      SELECT e.id,e.mkt_votum_vorgang_kz,e.art,e.typ,evb.status,e.anl_dat

      FROM entscheidungsvorlagen e

      JOIN ev_bearbeitungen evb ON (evb.entschv_id = e.id AND evb.aktuell_kz ='J')

      WHERE e.art = 4

        AND e.typ = 1

        AND evb.status >= 30

        AND evb.status < 50

        AND mkt_votum_vorgang_kz = 'N'

        ORDER BY anl_dat desc;

      

      

      UPDATE entscheidungsvorlagen SET mkt_votum_vorgang_kz ='J' WHERE id IN 

      (SELECT e.id

      FROM entscheidungsvorlagen e

      JOIN ev_bearbeitungen evb ON (evb.entschv_id = e.id AND evb.aktuell_kz ='J')

      WHERE e.art = 4

        AND e.typ = 1

        AND evb.status >= 30

        AND evb.status < 50

        AND mkt_votum_vorgang_kz = 'N')

       -- AND id = 169955

        ORDER BY anl_dat desc;
