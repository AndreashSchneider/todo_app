        FROM ev_batch_wertpaare

       WHERE EXISTS(SELECT 1

                      FROM ev_batch_auftraege

                     WHERE EV_BATCH_AUFTRAEGE.ID = EV_BATCH_WERTPAARE.evbauft_id

                     and NOT EXISTS

