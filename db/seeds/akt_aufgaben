      \r\n\
      <pre class=\"brush:ruby\">class Encrypter\r\n\
      # encrypter.rb\r\n # man übergibt eine Liste von Attributen\r\n # die verschlüsselt in die Datenbank geschrieben werden sollen \r\n  def initialize(attrs_to_manage)\r\n    @attrs_to_manage =attrs_to_manage \r\n  end\r\n  # Vor dem Speichern oder Aktualisieren werden die Daten \r\n  # mit dem wahnsinnig sicheren Cäsar-Verfahren verschlüssel.\r\n  def before_save(model)\r\n    @attrs_to_manage.each do |field| \r\n      model[field].tr!(\"A-z\",\"B-za\")\r\n    end\r\n  end\r\n  # Nach dem Speichern folgt wieder die Entschlüsselung\r\n  def after_save(model)\r\n    @attrs_to_manage.each do |field|\r\n      model[field].tr!(\"B-za\",\"A-z\")\r\n    end \r\n  end\r\n  # dasselbe beim Finden eines Eintrags\r\n  alias_method :after_find, :after_save\r\n\
      end\r\n\
      </pre>"
    mantis_nummer: 
    aufgabe_id: 
    id: "3807"
