          dbms_output.put_line('kf_kennzahl_text  '||i_t_dlm_jahrw(i).kf_kennzahl_text  );

          dbms_output.put_line('kf_ist_wert       '||i_t_dlm_jahrw(i).kf_ist_wert       );

          dbms_output.put_line('budget_aktuell    '||i_t_dlm_jahrw(i).budget_aktuell    );

          dbms_output.put_line('wert_max          '||i_t_dlm_jahrw(i).wert_max          );

          dbms_output.put_line('wert_min          '||i_t_dlm_jahrw(i).wert_min          );

          dbms_output.put_line('kz_summenzeile    '||i_t_dlm_jahrw(i).kz_summenzeile    );

          dbms_output.put_line('kz_freitext       '||i_t_dlm_jahrw(i).kz_freitext       );

          IF i_t_dlm_jahrw(i).kz_freitext IS NOT NULL THEN

            IF i_t_dlm_jahrw(i).kz_summenzeile ='N' THEN

             IF i_t_dlm_jahrw(i).kf_kennzahl_art<131 THEN

               dbms_output.put_line('Achtung Kandidat auf Fehler');

             END IF;

            

            END IF;

          END IF; 

      end loop;  

         

      END;
