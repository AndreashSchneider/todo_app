           , verwendungs_status

           , saldo_lfd

           , prog_name

           , entschv_id

        FROM PRODUKTVERWENDUNGEN

       WHERE     NOT EXISTS (SELECT 1

                               FROM PM_VERSIONEN

                              WHERE PRODUKTVERWENDUNGEN.PM_VER_ID = PM_VERSIONEN.ID)

             AND verwendungs_status = 'E'

             AND saldo_lfd > 0

             AND typ = 'IR'
    projekt: Ent.Vorlage
    heute_bearbeiten_kz: N
    erklaerung: ""
    erledigt_am: 2010-04-12 00:00:00
    reihenfolge: 
    termin: 2010-04-12 00:00:00
  attributes_cache: {}

