    information: "BEGIN \r\n  pk_bik5_debug_ev.pr_generate_ev_report(176210);\r\n\
      END;\r\n\
      \r\n\
      -- holt die EV aus denen über EV-Druckdaten die Werte ermittelt werden.\r\n  SELECT entschv_id\r\n    FROM vw_bik5_ev_uebersicht_all\r\n   WHERE     person_id IN (SELECT person_id\r\n                             FROM entscheidungsvorlagen\r\n                            WHERE ID = 177210)\r\n         AND entschv_id != 177210\r\n         AND status = 50\r\n         AND art != 'KRI'\r\n         AND entsch_vom_dat IS NOT NULL\r\n\
      ORDER BY entsch_vom_dat DESC;\r\n\
      \r\n\
      --- Liste der EVen mit doppelten Werten in der Tabelle ev_druckdaten !!\r\n\
      SELECT  entschv_id \r\n FROM ev_druckdaten \r\n\
      WHERE name IN ('SUM_ENT_GVOL','SUM_ENT_OBLIGO')\r\n\
      --AND entschv_id = 177117\r\n\
      GROUP BY entschv_id HAVING count(entschv_id)> 1;\r\n\
      ORDER BY anl_dat;"
