      v_t_kd_nr(106).kd_nr := 373820;

      v_t_kd_nr(107).kd_nr := 374143;

      v_t_kd_nr(108).kd_nr := 374208;

      v_t_kd_nr(109).kd_nr := 374210;

      v_t_kd_nr(110).kd_nr := 374531;

      v_t_kd_nr(111).kd_nr := 375008;

      v_t_kd_nr(112).kd_nr := 393048;

      v_t_kd_nr(113).kd_nr := 394386;

      v_t_kd_nr(114).kd_nr := 394478;

      v_t_kd_nr(115).kd_nr := 394600;

      v_t_kd_nr(116).kd_nr := 396918;

      v_t_kd_nr(117).kd_nr := 397429;

      v_t_kd_nr(118).kd_nr := 397623;

      v_t_kd_nr(119).kd_nr := 397714;

      v_t_kd_nr(120).kd_nr := 400496;

      v_t_kd_nr(121).kd_nr := 400543;

      v_t_kd_nr(122).kd_nr := 401285;

      v_t_kd_nr(123).kd_nr := 402557;

      v_t_kd_nr(124).kd_nr := 403285;

      v_t_kd_nr(125).kd_nr := 403468;

      v_t_kd_nr(126).kd_nr := 405589;

      v_t_kd_nr(127).kd_nr := 405743;

      

      v_t_kd_nr(40).lfd_nr := 2;

      v_t_kd_nr(51).lfd_nr := 2;

      v_t_kd_nr(82).lfd_nr := 2;

      

      

       FOR i IN v_t_kd_nr.FIRST .. v_t_kd_nr.LAST

       LOOP

        

       BEGIN

        SELECT pvw.id

          INTO v_n_pro_ver_id

          FROM produktverwendungen pvw

         WHERE pvw.kd_nr  = v_t_kd_nr(i).kd_nr 

           AND pvw.lfd_nr = v_t_kd_nr(i).lfd_nr 

           AND pvw.typ    = 'DEAL'

           AND pvw.verwendungs_status = 'Z'

           AND pvw.bearb_status <> 'E';

        

        --status in produktverwendungen auf 'E' aendern (erledigt)

        UPDATE produktverwendungen

           SET  prog_name    = 'EDIT Deal erledigr',

                bearb_status = 'E'

         WHERE id = v_n_pro_ver_id;

         

        -- DELETE p_v_deal_daten

        DELETE p_v_deal_daten WHERE kd_nr = v_t_kd_nr(i).kd_nr AND lfd_nr = v_t_kd_nr(i).lfd_nr;

      

        -- DELETE p_v_deal_ebitda_daten

        DELETE p_v_deal_ebitda_daten WHERE kd_nr = v_t_kd_nr(i).kd_nr AND lfd_nr = v_t_kd_nr(i).lfd_nr;

      

        -- DELETE dlm_termin_daten

        DELETE dlm_termin_daten WHERE pro_ver_id = v_n_pro_ver_id;

         

        -- Covenants trennen

        pk_sid2_ver_interface.pr_delete_pvw_details(v_n_pro_ver_id); 

         

        v_n_pro_ver_id := NULL;

        

          EXCEPTION

            WHEN NO_DATA_FOUND THEN   -- Location nicht gefunden, naechster deal

             NULL;

            WHEN OTHERS THEN     -- bei Fehler wird mit naechsten Deal fortgesetzt

      --       dbms_output.put_line('Fehler bei pro_ver_id:'||to_char(v_n_pro_ver_id)||' -'||SQLERRM);

             NULL;

         END;

        

       END LOOP;

      

      END;

      /

      

      

      

      

      


