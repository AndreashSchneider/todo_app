      CREATE OR REPLACE PROCEDURE pr_auto_test as

           PRAGMA AUTONOMOUS_TRANSACTION;

        CURSOR crdept IS 

        SELECT * FROM dept;

      BEGIN

         FOR rcdept IN crdept LOOP

           dbms_output.put_line(rcdept.deptno||'-'||rcdept.dname);

         END LOOP;

         INSERT INTO dept (deptno, dname, loc)

          VALUES (50, 'IT', 'DUESSELDORF');

         COMMIT;

      END; 

      

      DECLARE 

       test  exception;

      BEGIN

      -- die empno 7839 ist schon vorhanden

             INSERT INTO dept (deptno, dname, loc)

              VALUES (60, 'ET', 'DUESSELDORF');

          -- COMMIT; das Insert wird in der autonomen Transaktion erst gesehen,

          -- wenn commitet wird.

          pr_auto_test;

         -- RAISE test;

      /*EXCEPTION

          WHEN OTHERS THEN

            pr_auto_test;

          ROLLBACK;

          --RAISE ; */

      END;

      

      DELETE FROM dept WHERE deptno IN (50,60);

      SELECT * FROM dept;

      COMMIT;
