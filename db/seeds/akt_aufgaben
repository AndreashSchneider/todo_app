      \r\n\
      /********************************************************************************\r\n    -- |rpmax|Tslvt|Tsl01|Tsl02|Tsl03|... |Tsl14|Tsl15|Tsl16\r\n    --    16 |     |  100|                |      --> Änderung am 01.04. des GJ\r\n    --    32 |     |     |     |   20|    |      -->      \"   am 19.04  de  GJ (16+3)\r\n    --    .. |     |     |     | ..  |    |  ..\r\n    --   368 |     |     |     |     |    |  23 |--> Änderung am 31.03. im Schaltjahr\r\n\
      --*********************************************************************************/\r\n\
      \r\n\
      -- Geschäftsjahr ermitteln ->2009\r\n\
      SELECT PK_VER2_VERTRAG_DB.FK_GJAHR(20,to_date('28.02.2010','dd.mm.yyyy')) FROM dual;\r\n\
      SELECT PK_VER2_VERTRAG_DB.FK_GJAHR(20,to_date('02.03.2010','dd.mm.yyyy')) FROM dual;\r\n\
      \r\n\
      -- Tag im Geschäftsjahr(28.02)  ->334  (02.03) ->337\r\n\
      SELECT PK_VER2_VERTRAG_DB.FK_TAG_IM_GJAHR(20,to_date('28.02.2010','dd.mm.yyyy')) FROM dual;\r\n\
      \r\n\
