- !ruby/object:Aufgabe 
  attributes: 
    status: Offen
    zeit: 
    aend_dat: 
    erfasst: 2009-09-04 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: " Outlook mit Ruby auslesen \r\n\
      \r\n\
      require 'win32ole'\r\n\
      \r\n\
      outlook = WIN32OLE.new('Outlook.Application')\r\n\
      mapi = outlook.GetNameSpace('MAPI')\r\n\
      \r\n\
      #   Get list of available Address Lists\r\n\
      mapi.Session.AddressLists.each do |list|\r\n    puts list.Name\r\n\
      end\r\n\
      \r\n\
      #   Access an Address List:\r\n\
      address_list = mapi.Session.AddressLists('Contacts')\r\n\
      address_list = mapi.Session.AddressLists('Personal Address Book')\r\n\
      address_list = mapi.Session.AddressLists('Global Address List')\r\n\
      \r\n\
      #   Outlook security dialog will prompt to allow access to AddressEntries:\r\n\
      address_entries = address_list.AddressEntries\r\n\
      \r\n\
      #   Iterate over the AddressEntries collection:\r\n\
      address_entries.each do |address_entry|\r\n    if address_entry.Name =~ /Sinatra/\r\n        puts address_entry.Name, address_entry.Address\r\n    end\r\n\
      end\r\n\
      \r\n\
      #   Search for an Address:\r\n\
      address_entry = address_entries.Item(\"sinatra, frank\")\r\n\
      puts address_entry.Name\r\n\
      puts address_entry.Address\r\n"
    mantis_nummer: 
    aufgabe_id: 
    id: "2688"
    information: ""
    projekt: Ruby
    heute_bearbeiten_kz: N
    erklaerung: ""
    erledigt_am: 
    reihenfolge: 
    termin: 
  attributes_cache: {}

