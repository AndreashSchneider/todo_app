      IF satz.pvw_typ = 'UB' AND NVL (vcueberweis_termin, 1) = 2\r\n  THEN cashfl_id_tab (nzaehler).betrag := satz.betrag;\r\n  ELSE cashfl_id_tab (nzaehler).betrag := satz.offen_betrag;\r\n\
      END IF;\r\n\
      \r\n\
      -- ...oder fk_erzeuge_nostro_umbuchung\r\n\
      -- wird immer mit offen-Berag aufgerufen\r\n\
      FK_ERZEUGE_NOSTRO_UMBUCHUNG(RecNostro.offen_betrag, ; \r\n\
      \r\n\
      -- Es war der CF-Auftrag, der falsch angelegt wurde !!!\r\n\
