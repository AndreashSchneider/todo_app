      \t  }\r\n\
      \t  wo1\r\n  end\r\n  \r\n  sqltext '-- hier kommt die Maus select\r\n  select pk_testemich\r\n   from  produktverwendungen\r\n  wHere  update case break' \r\n  \r\n  text='  pk_testemich '\r\n  text.gsub!((/[^ ]*)pk_([^ \\.]*)/$1PK_$2/i/g)\r\n  p text\r\n  "
