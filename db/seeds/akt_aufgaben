    aend_dat: 2010-10-18 00:00:00
    erfasst: 2010-10-05 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: |-
      Reorganisation von pk_bik5_utils.pr_get_evol_offen

      

      * *pk_bik5_utils.pkb* eingecheckt, ikb2, ikb3

      * Procedure Layout angepasst !!
    mantis_nummer: 
    aufgabe_id: 
    id: "3776"
    information: "PROCEDURE pr_get_evol_offen( i_person_id  IN personen.id%type,\r\n                            o_sum_obligo OUT number,\r\n                            o_sum_gv     OUT number )\r\n   IS\r\n   v_R_OBLIGO     EV_VERRECHNUNGEN.OBLIGO%TYPE:=0;\r\n   v_V_OBLIGO     EV_VERRECHNUNGEN.OBLIGO%TYPE:=0;\r\n   v_X_OBLIGO     EV_VERRECHNUNGEN.OBLIGO%TYPE:=0;\r\n   v_R_GV         EV_VERRECHNUNGEN.OBLIGO%TYPE:=0;\r\n   v_V_GV         EV_VERRECHNUNGEN.OBLIGO%TYPE:=0;\r\n   v_X_GV         EV_VERRECHNUNGEN.OBLIGO%TYPE:=0;\r\n   v_obligo      EV_VERRECHNUNGEN.OBLIGO%TYPE:=0;\r\n   v_gv          EV_VERRECHNUNGEN.gesch_vol%TYPE:=0;\r\n   v_sum_obligo  number:=0;\r\n   v_sum_gv      number:=0;\r\n   BEGIN\r\n     -- Alle PVW mit Verwendung_status ='E'\r\n     FOR c1 IN (\r\n     SELECT pvw.id pvw_id ,\r\n          pk_glb0_kurse.fk_ermittle_waehr_betr(w.waehr_code , 'EUR', nvl(vw.obligo,0)  , trunc(SYSDATE)) obligo,\r\n          pk_glb0_kurse.fk_ermittle_waehr_betr(w.waehr_code , 'EUR', nvl(vw.gesch_vol,0)  , trunc(SYSDATE)) gesch_vol,\r\n          pvw.entschv_id          entschv_id,\r\n          pvw.t_waehr_id          waehr_id\r\n      FROM produktverwendungen pvw,\r\n           personen p,\r\n           vw_bik5_einzelev vw,\r\n           EV_BEARBEITUNGEN evb,\r\n           waehrungen w\r\n      WHERE pvw.VERWENDUNGS_STATUS = 'E'\r\n      AND   p.kd_nr =pvw.KD_NR\r\n      AND   p.id = i_person_id\r\n      AND   vw.pvw_id  = pvw.id\r\n      AND   vw.ENTSCHV_ID= pvw.entschv_id\r\n      AND   vw.ENTSCHV_ID = evb.entschv_id\r\n      AND   evb.status =50\r\n      AND   pvw.t_waehr_id = w.id ) LOOP\r\n      IF c1.pvw_id IS NOT NULL THEN\r\n        --- Reduzierung der Entscheidungsvorschläge\r\n        BEGIN\r\n          SELECT pk_glb0_kurse.fk_ermittle_waehr_betr(w.waehr_code , 'EUR', nvl(OBLIGO,0), \r\n                 trunc(SYSDATE)) obligo,\r\n                 pk_glb0_kurse.fk_ermittle_waehr_betr(w.waehr_code , 'EUR', nvl(GESCH_VOL,0) , trunc(SYSDATE)) GESCH_VOL\r\n            INTO v_R_OBLIGO, v_R_GV\r\n            FROM EV_VERRECHNUNGEN,\r\n                 waehrungen w\r\n          WHERE  ENTSCHV_ID =c1.entschv_id \r\n             AND PRO_VER_ID  = c1.pvw_id\r\n             AND VERRECH_TYP = 'R'\r\n             AND c1.waehr_id = w.id;\r\n        EXCEPTION WHEN NO_DATA_FOUND THEN\r\n                 v_R_OBLIGO:=0;\r\n                 v_R_GV    :=0;\r\n          WHEN OTHERS THEN RAISE;\r\n        END;\r\n        BEGIN\r\n            SELECT pk_glb0_kurse.fk_ermittle_waehr_betr(w.waehr_code , 'EUR', nvl(OBLIGO,0) ,trunc(SYSDATE)) obligo,\r\n                   pk_glb0_kurse.fk_ermittle_waehr_betr(w.waehr_code , 'EUR', nvl(GESCH_VOL,0), trunc(SYSDATE)) GESCH_VOL\r\n              INTO v_V_OBLIGO, v_V_GV\r\n              FROM ev_verrechnungen,waehrungen w\r\n             WHERE ENTSCHV_ID =c1.entschv_id \r\n               AND PRO_VER_ID  = c1.pvw_id\r\n               AND VERRECH_TYP = 'V'\r\n               AND c1.waehr_id = w.id ;\r\n          EXCEPTION\r\n          WHEN NO_DATA_FOUND THEN\r\n             v_V_OBLIGO:=0;\r\n             v_V_GV    :=0;\r\n          WHEN OTHERS THEN RAISE;\r\n        END;\r\n        BEGIN\r\n          SELECT pk_glb0_kurse.fk_ermittle_waehr_betr(w.waehr_code , 'EUR', nvl(OBLIGO,0), trunc(SYSDATE)) obligo,\r\n                 pk_glb0_kurse.fk_ermittle_waehr_betr(w.waehr_code , 'EUR', nvl(GESCH_VOL,0), trunc(SYSDATE)) GESCH_VOL\r\n            INTO v_X_OBLIGO, v_X_GV\r\n            FROM ev_verrechnungen, waehrungen w\r\n           WHERE entschv_id =c1.entschv_id \r\n             AND pro_ver_id  = c1.pvw_id\r\n             AND verrech_typ = 'X'\r\n             AND c1.waehr_id = w.id ;\r\n        EXCEPTION WHEN NO_DATA_FOUND THEN\r\n          v_X_OBLIGO:=0;\r\n          v_X_GV    :=0;\r\n          WHEN OTHERS THEN RAISE;\r\n        END;\r\n       \r\n\
      \r\n        v_obligo :=nvl((c1.obligo - v_R_OBLIGO - (v_V_OBLIGO - v_X_OBLIGO)), 0);\r\n        v_sum_obligo := v_sum_obligo + v_obligo;\r\n        dbms_output.put_line(c1.entschv_id||' v_sum_obligo: '||v_sum_obligo||' Obligo: '||c1.obligo||' - v_R_OBLIGO: '||v_R_OBLIGO||' - (v_V_OBLIGO: '||v_V_OBLIGO||' - v_X_OBLIGO: '||v_X_OBLIGO||')');        \r\n        v_gv :=nvl((c1.gesch_vol - v_R_GV - (v_V_GV - v_X_GV)), 0);\r\n        v_sum_gv := v_sum_gv + v_gv;\r\n      --  dbms_output.put_line(c1.entschv_id|| ' v_sum_gv: '   ||v_sum_gv     ||' GVol: ' ||c1.gesch_vol||' - v_R_GV: '  ||v_R_GV||' - (v_V_GV: '||v_V_GV||'v_X_GV: ' ||v_X_GV||')');\r\n      END IF;\r\n        v_obligo :=0;\r\n        v_R_OBLIGO:=0;\r\n        v_V_OBLIGO:=0;\r\n        v_X_OBLIGO:=0;\r\n        v_gv :=0;\r\n        v_R_GV:=0;\r\n        v_V_GV:=0;\r\n        v_X_GV:=0;\r\n     END LOOP;\r\n\
      \r\n      IF v_sum_obligo < 0 THEN\r\n        v_sum_obligo :=0;\r\n      END IF;\r\n      o_sum_obligo :=  v_sum_obligo;\r\n      IF v_sum_gv < 0 THEN\r\n       v_sum_gv :=0;\r\n      END IF ;\r\n      o_sum_gv := v_sum_gv;\r\n   END pr_get_evol_offen;"
