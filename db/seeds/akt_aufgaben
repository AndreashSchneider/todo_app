      \r\n function fk_neue_dokst_id_lesen \r\n return NUMBER\r\n is\r\n   v_nID NUMBER;\r\n begin\r\n   select V_DOKST_SEQ.NEXTVAL\r\n   into   v_nID\r\n   from   DUAL;\r\n   --\r\n   return v_nID;\r\n exception\r\n   when others then\r\n     return NULL;\r\n end;        \r\n \r\n procedure pr_insert(p_DOKST_ID       DOKUMENT_STRUKTUREN.ID%TYPE\r\n                    ,p_V_DOKST_ID     DOKUMENT_STRUKTUREN.V_DOKST_ID%TYPE\r\n                    ,p_DYNAMIK_KZ     DOKUMENT_STRUKTUREN.DYNAMIK_KZ%TYPE\r\n                    ,p_KUERZEL        DOKUMENT_STRUKTUREN.KUERZEL%TYPE\r\n                    ,p_SORT_NR        DOKUMENT_STRUKTUREN.SORT_NR%TYPE\r\n                    ,p_TABELLEN_NAME  DOKUMENT_STRUKTUREN.TABELLEN_NAME%TYPE\r\n                    ,p_TAC_NAME       DOKUMENT_STRUKTUREN.TAC_NAME%TYPE\r\n                    ,p_VERARB_ROUTINE DOKUMENT_STRUKTUREN.VERARB_ROUTINE%TYPE)\r\n is \r\n begin          \r\n   insert into  DOKUMENT_STRUKTUREN\r\n               (ID\r\n               ,ANL_DAT\r\n               ,ANL_USER\r\n               ,PROG_NAME\r\n               ,V_DOKST_ID\r\n               ,DYNAMIK_KZ\r\n               ,KUERZEL\r\n               ,SORT_NR\r\n               ,VERANLASSER\r\n               ,ANW_NAME\r\n               ,TABELLEN_NAME\r\n               ,TAC_NAME\r\n               ,VERARB_ROUTINE)\r\n   VALUES      (p_DOKST_ID\r\n               ,sysdate\r\n               ,v_canl_user\r\n               ,v_cprog_name\r\n               ,p_V_DOKST_ID\r\n               ,p_DYNAMIK_KZ\r\n               ,p_KUERZEL\r\n               ,p_SORT_NR\r\n               ,v_cveranlasser\r\n               ,v_canw_name\r\n               ,p_TABELLEN_NAME\r\n               ,p_TAC_NAME\r\n               ,p_VERARB_ROUTINE);  \r\n EXCEPTION\r\n   when dup_val_on_index then\r\n     dbms_output.put_line('Satz kann nicht eingefügt werden:'||p_Kuerzel);\r\n   WHEN others THEN\r\n     dbms_output.put_line('Fehler bei Insert:'||sqlerrm);\r\n     raise;\r\n END;                  \r\n \r\n