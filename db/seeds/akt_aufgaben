      -- Testbeispiel \r\n\
      --- Das Skript wirf für alle in Parameterdaten eingetragenen Personen den NL-Leiter wieder raus\r\n\
      --- Daher entspricht die Funktion nicht mehr ganz der in pk_bik5_mailing !!!\r\n\
      DECLARE \r\n  CURSOR crPers IS \r\n\
      SELECT per.kbez,per.pers_nr FROM personen per\r\n  WHERE EXISTS \r\n    (SELECT 1 \r\n      FROM parameterdaten p  \r\n      WHERE p.kuerzel IN ('NLLO','NLPA','NLMA','NLMI','NVLO','NVPA','NVMA','NVMI') \r\n        AND wert_varchar LIKE ('%'||per.pers_nr||'%'))\r\n  AND per.pers_nr >0;\r\n  FUNCTION fk_get_nc_nleiter(inPersNr number) RETURN varchar  IS \r\n   CURSOR crgetNlLeiter (inPersNr number) IS\r\n   SELECT substr(wert_varchar,1,instr(wert_varchar,'#')-1) NLLeiter \r\n    FROM parameterdaten \r\n   WHERE kuerzel IN ('NLLO','NLPA','NLMA','NLMI','NVLO','NVPA','NVMA','NVMI')\r\n     AND wert_varchar LIKE ('%'||inPersNr||'%');\r\n   NlLeiter NUMBER;\r\n   name varchar2(200);\r\n  BEGIN \r\n    OPEN crGetNlLeiter(inPersNr);\r\n    FETCH crGetNlLeiter INTO NlLeiter;\r\n    IF crGetNlLeiter%FOUND THEN  \r\n      SELECT kbez INTO name FROM personen WHERE pers_nr = NlLeiter;\r\n      CLOSE crGetNlLeiter;\r\n      -- RETURN NlLeiter;\r\n      RETURN name;\r\n      ELSE\r\n      CLOSE crGetNlLeiter;\r\n      RETURN Null;\r\n    END IF;\r\n  END; \r\n\
      BEGIN \r\n  FOR rcPers IN crPers LOOP \r\n    dbms_output.put_line(rcPers.kbez||';'||rcPers.pers_nr||';'||fk_get_nc_nleiter(rcpers.pers_nr));\r\n  END LOOP;\r\n\
      END;\r\n"
