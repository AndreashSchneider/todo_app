      decode(hr.objekt_typ,'R',t.kbez,hr.objekt_name),replace(ht.text,'"','') FROM  hilfe_reports hr

      JOIN vw_glb1_transaktionen_dbs t ON (hr.v_tacdb_id =t.id) 

      JOIN hilfe_texte ht ON (ht.v_hilre_id = hr.id)

      JOIN vw_glb1_anwendungen anw ON (anw.id = t.v_anw_id)

         WHERE  sprache = 'GB'

