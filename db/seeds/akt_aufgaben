      \r\n\
      # neues Feld Kundennummer(Pflichtfeld)\r\n\
      # neues Feld Entscheidungsdatum (Pflichtfeld)\r\n\
      # Auswertung EV enschieden *am* durch *KdNr*\r\n\
      # Herr Gahner macht eine EAB bis in zwei Wochen\r\n\
      # Umsetzung geplant bis Ende Juni\r\n\
      # Datenmodelländerunge in Kontroll_fristen\r\n\
      \r\n\
      * Erinnerungsmail an Herrn Gahner am 11.05.\r\n\
      * geht am 15.07 auf Abnahme "
    mantis_nummer: "41135"
    aufgabe_id: "3653"
    id: "3462"
    information: "-- Prüfen auf Gruppenverknüpfung\r\n\
      pr_check_Gruppenverknuepfung\r\n  pk_bik5_bestandsfristen.pr_set_vw_bik5_neu_bfrist_ev(i_entschv_id);\r\n\
      -- .. wird ein Parameter für die View vw_bik5_neu_bfrist_ev gesetzt\r\n\
      vw_bik5_neu_bfrist_ev\r\n\
      -- die View erhält ihre Daten aus der Funktion \r\n\
      FROM TABLE( \r\n  PK_BIK5_BESTANDSFRISTEN.fk_get_BestandsfristTC\r\n       (pk_bik5_bestandsfristen.fk_get_vw_bik5_neu_bfrist_ev)\r\n  \r\n  \r\n\
      fk_get_BestandsfristTC\r\n\
      \r\n\
      1.\r\n\
      --------------------------------------------------------------------------------     \r\n\
      TYPE to_bik5_befristung AS OBJECT (\r\n       person_id     NUMBER(8,0)   -- PERSONEN.ID%TYPE    -- PK aus Personen der Bestandsfristsetzung\r\n     , kd_nr       NUMBER(6,0)   -- PERSONEN.KD_NR%TYPE -- Kundennummer zur Bestandsfristsetzung\r\n     , kbez            VARCHAR2(80)  -- PERSONEN.KBEZ%TYPE  -- Kundenkurzbezeichnung\r\n     , grund             VARCHAR2(200) -- Grund der Bestandsfristsetzung)\r\n\
      --------------------------------------------------------------------------------     \r\n     , ev_kd_nr\r\n\
      --------------------------------------------------------------------------------     \r\n\
      \r\n\
      --------------------------------------------------------------------------------     \r\n\
      2. vw_bik5_neu_bfrist_ev\r\n\
      PERSON_ID, KD_NR, KBEZ, GRUND, ev_kd_nr   \r\n\
      --------------------------------------------------------------------------------     \r\n\
      \r\n\
      \r\n\
      3. pk_bik5_bestandsfristen.fk_get_BestandsfristTC  \r\n\
      \r\n  v_to_out := TO_BIK5_BEFRISTUNG(  person_id => c1.person_id\r\n                                  ,  kd_nr     => c1.kd_nr\r\n                                  ,  kbez      => c1.kbez\r\n                                  ,  grund     => vc_temp\r\n\
      --                                ,  ev_kd_nr  => n_kd_nr_orig                                  \r\n                                  );\r\n\
      --------------------------------------------------------------------------------     \r\n\
      \r\n\
      4. Tabelle kontroll_fristen anpassen\r\n\
      \r\n  ev_person_id einfügen\r\n\
      \r\n\
      --------------------------------------------------------------------------------  \r\n\
      \r\n\
      5. Maske anpassen\r\n\
      \r\n\
      6. Report anpassen                                     \r\n\
      \r\n\
      \r\n\
      -- Die Abfrage erzeugt eine Liste von 1589 Einträgen und versucht die Kundennummer\r\n\
      -- zu ermitteln !!\r\n\
      \r\n\
      SELECT k.id\r\n     , k.bestand_frist_dat\r\n     , per.kd_nr\r\n     , k.bemerkung\r\n     , REGEXP_SUBSTR(k.bemerkung, '\\(\\d*\\)\\') \"aus (123)\"\r\n     , REGEXP_SUBSTR(k.bemerkung, '[Kk][Dd]\\s?\\d{1,3}(\\.|\\s)?\\d{3}')\r\n         \"Kd 12 .34\"\r\n     , REGEXP_SUBSTR(k.bemerkung, '\\s\\d{3,6}\\s') \"ohne OA oder Kd \"\r\n     , REGEXP_SUBSTR(k.bemerkung, '(see|siehe|sieh|arr|ARR)\\s?\\d{3,6}')\r\n         \"see, siehe, ARR 1234 \"\r\n     , REGEXP_SUBSTR(k.bemerkung, '\\d{1,3}\\.\\d{3,3}\\s') \"Zahlen mit Punkt\"\r\n  FROM   kontroll_fristen k\r\n       JOIN\r\n         personen per\r\n       ON (k.person_id = per.id)\r\n WHERE (   REGEXP_LIKE((bemerkung), '.*\\(\\d*\\)') --liefert die Verklammerten Kundennummern (968 Fälle)\r\n        OR REGEXP_LIKE((bemerkung), '\\d{1,3}\\.\\d{3,3}\\s') --liefert die Zahlen mit Punkt ohne Datum ( 18 Fälle =>alle GLB6S300) --> haben alle Kd vor der Zahl stehen!!\r\n        OR REGEXP_LIKE((bemerkung), '[Kk][Dd]\\s?\\d{1,3}\\.?\\d{3}') --liefert die Zahlen nach Kd/KD/kd        (519 Fälle =>alle GLB6S300)\r\n        OR( REGEXP_LIKE((bemerkung), '\\s\\d{3,6}\\s') --liefert die Zahlen ohne Kd und OA       ( 14 Fälle =>alle GLB6S300) --> Korrektur KD davor schreiben\r\n           AND INSTR(bemerkung, 'KD') = 0\r\n           AND INSTR(bemerkung, 'OA') = 0)\r\n        OR REGEXP_LIKE((bemerkung), '(see|siehe|sieh|arr|ARR)\\s?\\d{3,6}') --liefert Zahlen nach see,arr oder ARR   ( 91 Fälle => alle GLB6S300)\r\n                                                                         )\r\n       AND k.erfassung_dat IN (SELECT MAX(erfassung_dat)\r\n                                 FROM kontroll_fristen\r\n                                WHERE person_id = per.id);\r\n\
      \r\n\
      \r\n\
      SELECT regexp_substr('aas 11.12.2004','[^.]\\d{3,}\\.?\\d{0,3}\\') FROM dual;\r\n"
