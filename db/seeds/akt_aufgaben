- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 2011-02-21 00:00:00
    erfasst: 2010-11-05 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "automatische Hilfegenerierung "
    mantis_nummer: 
    aufgabe_id: 
    id: "3833"
    information: "# dsl zur Anlage von Html-Seiten. Methoden sind:\r\n\
      # hilfe ruft das Objekt auf und erzeugt den Titel\r\n\
      # modul erstellt einen Absatz, falls noch nicht vorhanden \r\n\
      # wenn der Absatz vorhanden ist, dann wird der Kommentar hinzugefügt.\r\n\
      # mit_feld erstellt ein Feld, innerhalb eines Absatzes, falls noch nicht vorhanden\r\n\
      # todo: Doppelte Feldhilfen verhindern.\r\n\
      \r\n\
      require 'rubygems'\r\n\
      require 'rake'\r\n\
      \r\n class Hilfe  \r\n   attr_reader :links,:absaetze,:forms,:feldhilfen\r\n   # behält woher die Einträge kommen, womit die Absätze/Module/Blöcke gekennzeichnet werden\r\n   # das wird beim Einlesen über die eingelesene Datei gesteuert dateiname_FMB -> 'fmb' und dateiname_fach -> 'fach'\r\n   attr_accessor :fachabt_oder_maske\r\n   \r\n   def initialize\r\n     @absaetze=[]\r\n     @links=[]\r\n     @feldhilfen=[]\r\n     @absatz_aktuell\r\n     @formsmodule={}\t\r\n     @fachabt_oder_maske='fach'\r\n   end\r\n   \r\n   def leeren\r\n     @absaetze=[]\r\n     @links=[]\r\n     @feldhilfen=[]\r\n     @absatz_aktuell=nil\r\n   end\r\n   \r\n   def add_titel(ueberschrift='Kein Titel definiert !',formsmodul='bik5s100')\r\n     @capital=Capital.new(titel=ueberschrift)\r\n     @forms =formsmodul\r\n     @formsmodule[formsmodul]=ueberschrift \r\n   end\r\n   def add_absatz (absatz,is_submodul=false)\r\n     @absaetze << absatz\r\n     @absatz_aktuell = absatz\r\n     @links << Link.new(absatz.absatz_ueberschrift,is_submodul,absatz.mask_vs_fachabt)\r\n   end\r\n  \r\n   def absatz_vorhanden? (absatz_name2,absatztext)\r\n     testausdruck =absatz_name2.downcase.gsub(/\\s/,'_')\r\n     @absaetze.each do |modul|\t\r\n       if modul.absatz_name==testausdruck then \r\n         @absatz_aktuell=modul\r\n         # wird dem Absatz ein geänderter Text mitgegeben, diesen anpassen\r\n         if absatztext!= 'vorhanden?' then\r\n           modul.text=absatztext\r\n         end            \r\n         return true\r\n       end\r\n     end\r\n     return false\r\n   end\r\n   def letzter_absatz \r\n     @absaetze.last\r\n   end\r\n  \r\n   def absatz_aktuell\r\n     @absatz_aktuell\r\n   end\r\n  def add_feldhilfe (feldname,sprungziel,beschreibung,forms)\r\n    a=Feldhilfe.new(feldname,sprungziel,beschreibung,forms)\r\n    @feldhilfen<<a\r\n  end \r\n  \r\n  def feldhilfen\r\n    @feldhilfen\r\n  end\r\n  def pruefe\r\n    # feldhilfen dürfen nicht den gleichen Namen wie Absätze haben\r\n    @absaetze.each do |absatz|\r\n    \tabsatz.absatz_ueberschrift \r\n    end\r\n  end\r\n  def schreib_uebersicht (titel)\r\n   s= \"<!DOCTyp HTML PUBLIC '-//W3C//DTD HTML 4.0 Transitional//EN'>\\n<HTML>\\n  \r\n   <HEAD>\\n<title>KreDa-#{@forms[0..2].upcase} Hilfe</title>\\n\r\n\
      \r\n   <link rel='stylesheet' href='css/div-einruecken01-2.css' type='text/css'> \r\n      <script type='text/javascript' src='js/prototype.js'></script>\r\n      <script type='text/javascript' src='js/scriptaculous.js?load=effects,builder'></script>\r\n      <script type='text/javascript' src='js/lightbox.js'></script>\r\n      <link rel='stylesheet' href='css/lightbox.css' type='text/css' media='screen' />\r\n    </head>\\n\r\n    <body>\"\r\n    s+=\"<h1>#{titel}</h1>\"\r\n    s+=\"<img class='links' src='images/hilfetexte_#{@forms[0..2]}.gif' alt='' border='0' ><br>\" \r\n    s+= \"<div id='menu'>\\n <br>\\n <ul>\"\r\n    \r\n    @formsmodule.each_pair do |k,v|\r\n    \tref=k+'.html'\r\n    \ts+=\"<li><a href='#{ref}'>#{v}</a></li>\\n\"   \t\r\n    end\r\n    s+=\" </div>\"\r\n    s+=\"\\n     </div>\\n </BODY>\\n</HTML>\"\r\n  end\r\n  \r\n  def schreib(mit_fmb_hilfe='N') # kann mit oder ohne fmb-Hilfe erzeugt werden\r\n   s= \"<!DOCTyp HTML PUBLIC '-//W3C//DTD HTML 4.0 Transitional//EN'>\\n<HTML>\\n  \r\n   <HEAD>\\n<title>KreDa-#{@forms[0..2].upcase} Hilfe</title>\\n\"\r\n   if mit_fmb_hilfe=='N' then\r\n     s+=\" <a href=file:///X:/test/Hilfe_automa_aktualisieren/#{@forms}_entwickler.html>*</a>\" \r\n   else \r\n     s+=\" <a href=file:///X:/test/Hilfe_automa_aktualisieren/#{@forms}.html>*</a>\" \r\n   end     \r\n   s+=\"\r\n   <link rel='stylesheet' href='css/div-einruecken01-2.css' type='text/css'> \r\n      <script type='text/javascript' src='js/prototype.js'></script>\r\n      <script type='text/javascript' src='js/scriptaculous.js?load=effects,builder'></script>\r\n      <script type='text/javascript' src='js/lightbox.js'></script>\r\n      <link rel='stylesheet' href='css/lightbox.css' type='text/css' media='screen' />\r\n    </head>\\n\r\n    <body>\r\n    \r\n    <div id='alles'>\"\r\n    s+=\"<img class='links' src='images/hilfetexte_#{@forms[0..2]}.gif' alt='' border='0' ><br>\"    \r\n    s+= @capital.schreib(mit_fmb_hilfe)\r\n\
      \r\n    s+=\"<div id='menu'>\\n <br>\\n <ul>\"\r\n     @links.each do |verweis| \r\n     \t# Hilfe mit oder ohne Links aus dem Formsmodul schreiben\r\n     \tif mit_fmb_hilfe=='J' or verweis.mask_vs_fachabt =='fach' then\r\n     \t  s +='    '+verweis.schreib\r\n     \tend\r\n     end\r\n     \t\r\n    s+= \"</ul>\\n </div>\"\r\n    s+= \" <div id='content'>\"\r\n    absatznamen=[]\r\n     # Hilfe mit oder ohne Blöcke/Module/Absätze aus dem Formsmodul schreiben\r\n     @absaetze.each do |absatz_tmp| \r\n     \tif mit_fmb_hilfe=='J' or absatz_tmp.mask_vs_fachabt =='fach' then     \t\r\n     \t  s +='    '+absatz_tmp.schreib\r\n     \t  absatznamen<< absatz_tmp.absatz_ueberschrift\r\n      end\r\n     \t  \r\n     end\r\n\
      \r\n    # Text durch Links ersetzen, solange der Name nicht mit einer Absatzüberschrift identisch ist!\r\n    @feldhilfen.each do  |feldhilfe| \r\n    \t# kommt der feldname in den Absatztiteln vor\r\n    \tunless absatznamen.grep(/#{feldhilfe.feldname}/).length >0    \t \r\n    \t  s = s.gsub(feldhilfe.feldname,feldhilfe.referenz)  \r\n    \telse \r\n    \t puts \"\\n------------------Achtung !!!------------------------------------\r\n  das Feld #{feldhilfe.feldname} kommt in den Absatztiteln vor und kann daher nicht referenziert werden ! \\n   \r\n    \t           Bitte diese Feldhilfe umbenennen!\r\n\
      ------------------Achtung !!!------------------------------------\"\r\n      end \t\r\n    end\r\n    \t\r\n    s+=\"<dl>\"\r\n    @feldhilfen.each{ |text| s +='    '+text.schreib}\r\n    s+=\" </dl>\"\r\n    s+=\" </div>\"\r\n    s+=\"\\n     </div>\\n </BODY>\\n</HTML>\"\r\n\
      \r\n  end\r\n    \r\n end \r\n     \r\n class Capital\r\n   def initialize(capital_text)\r\n    @capital_text=capital_text\r\n   end\r\n   def schreib(mit_fmb_hilfe)\r\n     s=\"<h1>#{@capital_text}</h1>\"\r\n   end\r\n end        \r\n\
      \r\n class Link\r\n   attr_accessor :mask_vs_fachabt\r\n   def initialize(link_text,submodul=false,mask_vs_fachabt='fach')\r\n     @mask_vs_fachabt=mask_vs_fachabt\r\n     @link_text=link_text\r\n     @submodul=submodul\r\n     @ref =@link_text.downcase.gsub(/\\s/,'_')\r\n     \r\n   end\r\n   def link_text_menue\r\n     if @link_text.length < 35 \r\n       return @link_text\r\n     else\r\n       return @link_text[0..35]+'..'\r\n     end\r\n   end\r\n   def schreib\r\n     if @submodul==false then\r\n       s=\"<li><a href='##{@ref}'>#{link_text_menue}</a></li>\"    \r\n     else\r\n       s=\"<li id=submodul><a href='##{@ref}'>#{link_text_menue}</a></li>\"\r\n     end    \r\n   end\r\n end\r\n\
      \r\n class Absatz \r\n   attr_reader :absatz_ueberschrift,:felder,:maske\r\n   attr_accessor :text,:submodul,:mask_vs_fachabt\r\n   def initialize(absatz_ueberschrift,text,submodul=false,maske='bik5s100',mask_vs_fachabt='fmb',ueberschr_2='')\r\n     @absatz_ueberschrift=absatz_ueberschrift\r\n     @maske = maske\r\n     @text=text\r\n     @ueberschr_2= ueberschr_2\r\n     @felder=[]\r\n     @submodul=submodul\r\n     @mask_vs_fachabt=mask_vs_fachabt\r\n   end\r\n   \r\n   def add_feld (feld)\r\n     @felder << feld\r\n   end\r\n   \r\n   def absatz_name\r\n     @absatz_ueberschrift.downcase.gsub(/\\s/,'_').gsub(/,/,'_')\r\n   end\r\n   \r\n   def bild_anzeigen\r\n     #  p \"Gesucht wird nach images/#{@maske}*#{absatz_name}.gif\"\r\n     Dir[\"images/#{@maske}*#{absatz_name}.gif\"].each do |datei|\r\n     #\tp \"..und gefunden wurde #{datei}\"\r\n     \treturn \"<a href='gif_gross/#{@maske}_#{absatz_name}.gif' rel='lightbox' title='#{@absatz_ueberschrift}'>\r\n     \t<img class='links' src='images/#{@maske}_#{absatz_name}.gif' alt='' width='150' border='0' height='113'/></a>\"\r\n     end   \r\n     ''\r\n   end\r\n   def schreib \r\n     if @submodul==true then \r\n       s= \" <A NAME='#{@absatz_ueberschrift.downcase.gsub(/\\s/,'_')}'><h2>#{@absatz_ueberschrift}</h2></A>\"\r\n     else\r\n       s= \" <A NAME='#{@absatz_ueberschrift.downcase.gsub(/\\s/,'_')}'><h3>#{@absatz_ueberschrift}</h3></A>\"\r\n     end\r\n     s+= \"\\n <p>  #{@ueberschr_2}\"\r\n     s+= \"#{bild_anzeigen} #{@text} </p>\\n <dl>\"    \r\n      @felder.each { |feld| s+= feld.schreib}\r\n     s+= \"</dl>\\n <a href='javascript:history.back()'>Zurueck</a> | <a href='#Top'>Anfang</a>\"                                                          \r\n   end \r\n end \r\n  \r\n class Feld  \r\n   def initialize(name,erklaerung,absatz_name,pos)\r\n     @name,@erklaerung,@absatz_name,@pos = name,erklaerung,absatz_name.downcase.gsub(/\\s/,'_'),pos\r\n   end\r\n   def schreib\r\n    # Mantis Nr(max6-Stellen) ersetzen durch Link\r\n    # engine = Haml::Engine.new(\"#{@erklaerung}\")\r\n    # @erklaerung=engine.render\r\n    @erklaerung=@erklaerung.gsub(/(Mantis:?) *?(\\d{5,6})/i,'<a href=http://mantis.ikb.de/view.php?id=\\\\2>*</a>')\t\r\n    s= \"<dt> #{@name}</dt>\\n <dd>#{@erklaerung}</dd>\"\r\n   end  \r\n end\r\n \r\n class Feldhilfe\r\n   attr_reader :feldname\r\n   def initialize(feldname,sprungziel,beschreibung,forms)\r\n    @feldname,@sprungziel,@beschreibung,@forms=feldname,sprungziel,beschreibung,forms\r\n   end\r\n   def referenz\r\n     \" <A href='##{@sprungziel.downcase.gsub(/\\s/,'_')}'>#{@feldname}</A>\"\r\n   end\r\n   def schreib\r\n     s= \" <dt><A NAME='#{@sprungziel.downcase.gsub(/\\s/,'_')}'>#{@feldname}</A> </dt>\"\r\n     s+= \"<dd>#{bild_anzeigen}#{@beschreibung}</dd>\"\r\n     \r\n     s+= \"\\n <a href='javascript:history.back()'>Zurueck</a> | <a href='#Top'>Anfang</a>\"  \r\n   end\r\n   \r\n   def feld_hilfe_name\r\n     @feldname.downcase.gsub(/\\s/,'_').gsub(/,/,'_')\r\n   end\r\n   \r\n   \r\n   def bild_anzeigen\r\n     # p \"Gesucht wird nach images/#{@forms}*#{feld_hilfe_name}_hilfe.gif\"\r\n     Dir[\"images/#{@forms}*#{feld_hilfe_name}_hilfe.gif\"].each do |datei|\r\n     #\tp \"..und gefunden wurde #{datei}\"\r\n     \treturn \"<a href='gif_gross/#{@forms}_#{feld_hilfe_name}_hilfe.gif' rel='lightbox' title='#{feldname}'>\r\n     \t<img class='links' src='images/#{@forms}_#{feld_hilfe_name}_hilfe.gif' alt='' width='130' border='0' height='98'/></a><br>\"\r\n     end   \r\n     # p \"Nicht gefunden wurde  images/#{@forms}*#{feld_hilfe_name}_hilfe.gif\"\r\n     ''\r\n   end\r\n   \r\n end\r\n\
      # --------------------Ausführungsteil --------------------------------------------------------------\r\n  @hilfeseite =Hilfe.new\r\n  def als_submodul\r\n    true\r\n  end\r\n  def hilfe (titel,forms='Bik5s100')\r\n    @hilfeseite.add_titel(titel,forms)\r\n  end\r\n\
      \r\n  def block (ueberschrift,text='vorhanden?',mask_vs_fachabt='fmb',submodul=false)\r\n    unless @hilfeseite.absatz_vorhanden?(ueberschrift,text) then \r\n      # Absatz anlegen\r\n      absatz_neu = Absatz.new(ueberschrift,text,submodul,@hilfeseite.forms,@hilfeseite.fachabt_oder_maske)\r\n      @hilfeseite.add_absatz absatz_neu,submodul\r\n    end\r\n  end\r\n  alias modul block\r\n  \r\n  def mit_feld (feldname,beschreibung,absatz=@hilfeseite.absatz_aktuell.absatz_name,pos=1)\r\n    @hilfeseite.absatz_aktuell.add_feld(Feld.new(feldname,beschreibung,absatz,pos))\r\n  end\r\n  alias und mit_feld  \r\n  \r\n  def feldhilfe (feldname,sprungziel,beschreibung)\r\n    @hilfeseite.add_feldhilfe(feldname,sprungziel,beschreibung,@hilfeseite.forms)\r\n  end\r\n  \r\n  def fachabt_oder_maske (fach)\r\n    @hilfeseite.fachabt_oder_maske=fach\r\n  end\r\n\
      \r\n  \r\n \r\n  \r\n  \r\n  def generiere_html(filename,fmb='J')\r\n    feldhilfentitel=[] \r\n    absatztitel=[]\r\n    fach_hilfe =''\r\n    # TEXT-FMB-File lesen und help-Anweisungen in Textfile speichern\r\n    forms= File.read(filename+'_FMB')\r\n    # das File der Fachabteilung einlesen, falls vorhanden\r\n    if File.exist?(\"#{filename}_fach.txt\") then\r\n      fachabteilungs_hilfe= File.read(File.dirname(__FILE__)+\"/#{filename}_fach.txt\")\r\n      fach_hilfe = 'J'\r\n    end\r\n    # fmb-file durchsuchen und mit evt. Fachabt.hilfe zusammengenerieren\r\n    File.open(File.dirname(__FILE__)+\"/#{filename}.txt\",'w') do |file|\r\n    \t# Text zwischen /**help ..*/ raussuchen\r\n    \tforms.scan(/\\/\\*\\*help(.*?)\\*\\//m) do |in_forms_gefunden|\r\n    \t\tif fach_hilfe == 'J' then\r\n    \t\t  p 'Fachhilfe gefunden und wird eingebunden..'   \t\t  \r\n    \t\t  vcfach_vs_fmb = \"fachabt_oder_maske 'fach'\\n\"\r\n    \t\t  masken_hilfe =vcfach_vs_fmb+ fachabteilungs_hilfe  \r\n    \t\t  vcfach_vs_fmb = \"fachabt_oder_maske 'fmb'\\n\"\r\n    \t\t  masken_hilfe+=vcfach_vs_fmb+in_forms_gefunden.to_s.sub(/^\\s+hilfe.*$/,'')\r\n    \t\telse\r\n    \t\t  p 'keine Fachhilfe gefunden!'\r\n    \t\t  vcfach_vs_fmb = \"fachabt_oder_maske 'fmb'\\n\"\r\n          masken_hilfe = vcfach_vs_fmb+in_forms_gefunden\r\n    \t\tend\r\n    \t\tfile.write \"#{masken_hilfe}\"\r\n    \tend\r\n    end   \r\n    \r\n    # Aus dem generierten Textfile zB. abw1s001.txt wird eine HTML-Seite generiert\r\n    load File.dirname(__FILE__)+\"/#{filename}.txt\"\r\n    # html-Seite inklusive fmb-Kommentare\r\n    if fmb=='J' then \r\n      filename=filename+'_entwickler'\r\n    end\r\n    p fmb\r\n    p filename\r\n    File.open(File.dirname(__FILE__)+\"/#{filename}.html\",'w') do |file|\r\n    \t file.write(@hilfeseite.schreib(fmb))\r\n    end     \r\n    \r\n    # Wird das hier noch gebraucht ??\r\n    # @hilfeseite.feldhilfen.each do |feldhilfe|\r\n    # \tfeldhilfentitel << feldhilfe.feldname\r\n    # end\r\n    # @hilfeseite.absaetze.each do |absatz|\r\n   \t#  absatztitel<< absatz.absatz_ueberschrift\r\n    # end\r\n \r\n    @hilfeseite.leeren\r\n  end\r\n  \r\n  # Schreibt in in filename eine Übersicht der Angelegten Hilfen (add_titel)\r\n  def generiere_uebersicht(filename,titel) # abw,bik oder dlm\r\n    File.open(File.dirname(__FILE__)+\"/#{filename}.html\",'w') do |file|\r\n    \tfile.write(@hilfeseite.schreib_uebersicht(titel))\r\n    end\r\n  end\r\n\
      \r\n  \r\n  # macht aus dem SQL-Template-Skript und den Filenamen (z.B. abw1s001) ein Skript zum Einfügen Hilfereferenzen\r\n  def generiere_sql(filename)\r\n     # template aus dem Verzeichnis holen \r\n     template_= File.read(File.dirname(__FILE__)+\"/referenzen_template.txt\")\r\n     File.open(File.dirname(__FILE__)+\"/#{filename}.sql\",'w') do |file|\r\n     \tfile.write(template_.gsub('ersetzen_anw_name',filename[0..2].upcase).gsub('ersetzen_tac_name',filename[3..7].upcase)) \t\r\n     end\r\n  end  \r\n  \r\n  \r\n  # holt sich alle sql-Dateien und führt sie auf der IKB3 aus !\r\n  def schreibe_in_db\r\n    FileList[\"#{File.dirname(__FILE__)}/**/*.sql\"].each do |sql_datei|\r\n     system \"sqlplus s210005/ikbs@ikb3 @#{sql_datei}\"\r\n    end\r\n  end\r\n\
