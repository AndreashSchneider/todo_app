- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2008-10-02 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Skript zur Kontenanlage über den HUB"
    mantis_nummer: 
    aufgabe_id: 
    id: "1974"
    information: |
      DECLARE

      

          bGesperrt BOOLEAN;

          vcDummy   VARCHAR2(20);

          vMessage  pk_glb0_konten.tMessage;

          vKonto    pk_glb0_konten.tKonto;

          nRet      NUMBER;   

      

      Begin

      -- Kontenparameter belegen

      vKonto   := pk_glb0_konten.fk_get_name_vertrag_id(1597142); 

      

      -- Ausgabe Kontoparameter:

      dbms_output.put_line('vKonto.vcTyp     : '||vKonto.vcTyp);

      dbms_output.put_line('vKonto.nKdNr     : '||to_char(vKonto.nKdNr));

      dbms_output.put_line('vKonto.nLfdNr    : '||to_char(vKonto.nLfdNr));

      dbms_output.put_line('vKonto.nTranche  : '||to_char(vKonto.nTranche));

      dbms_output.put_line('vKonto.nUbTyp    : '||to_char(vKonto.nUbTyp));

      dbms_output.put_line('vKonto.nUbNr     : '||to_char(vKonto.nUbNr));

      dbms_output.put_line('vKonto.nProVerId : '||to_char(vKonto.nProVerId));

      dbms_output.put_line('vKonto.nRefNr    : '||to_char(vKonto.nRefNr));

      dbms_output.put_line('vKonto.nSaldoLfd : '||to_char(vKonto.nSaldoLfd));

      dbms_output.put_line('vKonto.vcName1   : '||vKonto.vcName1);

      dbms_output.put_line('vKonto.vcUbVerTyp: '||vKonto.vcUbVerTyp);

      

      

      -- Wenn Konto existiert

      IF PK_GLB0_KONTEN.FK_KONTO_EXISTS(vKonto,bGesperrt,vMessage) THEN

       dbms_output.put_line('Konto existiert');

      ELSE

       dbms_output.put_line('Konto existiert nicht -- anlegen');

      -- /*

          IF NOT PK_GLB0_KONTEN.FK_KONTO_ANLEGEN(vKonto,NULL,FALSE,'TESTUB99',vcdummy,vMessage) THEN       

              --o_nErr := 12350;

              --o_bOk := FALSE;

              dbms_output.put_line('Konto anlegen mit Fehler');

              IF vMessage.nNR IS NOT NULL THEN

                 dbms_output.put_line('vMessage.nNr   : '||to_char(vMessage.nNr));

                 dbms_output.put_line('vMessage.vcPar1: '||vMessage.vcPar1);

                 dbms_output.put_line('vMessage.vcPar2: '||vMessage.vcPar2);

                 dbms_output.put_line('vMessage.vcPar3: '||vMessage.vcPar3);

      

              END IF; 

          dbms_output.put_line('Konto anlegen: TRUE'); 

          END IF;

      --  */

      END IF;

      

      

      commit;

      

      

      dbms_output.put_line('Normales Ende ohne fehler');

      

      exception

      when OTHERS then

      dbms_output.put_line('Fehler '||SQLERRM);

      End;

      


