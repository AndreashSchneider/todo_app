      \r\n\
      DECLARE \r\n  entschv_id NUMBER:=162087;\r\n  btest BOOLEAN;\r\n\
      BEGIN \r\n\
      \r\n\
      /* Wenn Marktvotum schon in EV-Bearbeitung, kann trotzdem ein \r\n Marktvotumsreport erstellt werden, wenn dass Verwendung_kz umgeswitch wird\r\n\
      */\r\n\
      UPDATE entscheidungsvorlagen\r\n   SET verwendung_kz = 3\r\n WHERE id = entschv_id;\r\n\
      btest := pk_bik5_utils.fk_makvotum_pdf_u_management(entschv_id);\r\n\
      \r\n\
      -- Zurück in die Entscheidungsvorlage\r\n\
      UPDATE entscheidungsvorlagen\r\n   SET verwendung_kz = 1\r\n WHERE id = entschv_id;\r\n END;\r\n COMMIT;"
