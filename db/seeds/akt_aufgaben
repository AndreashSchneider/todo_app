    mantis_nummer: 
    aufgabe_id: 
    id: "2352"
    information: "DECLARE \r\n\
      CURSOR crVertraege IS\r\n SELECT * FROM vw_ver0_pvw_irbrg\r\n WHERE \r\n ((kd_nr = 183450 and lfd_nr =25 and Tranche =0) or\r\n\
      (kd_nr = 020569 and lfd_nr =8 and Tranche =0 ) or\r\n\
      (kd_nr = 142420 and lfd_nr =2 and Tranche =0 ) or\r\n\
      (kd_nr = 125550 and lfd_nr =9 and Tranche =0 ) or\r\n\
      (kd_nr = 126903 and lfd_nr =1 and Tranche =61) );\r\n nsaldo_aktuell NUMBER:=0;\r\n\
      BEGIN \r\n\
      FOR rcVertraege IN crVertraege LOOP \r\n        IF rcVertraege.extern_kz ='N' THEN\r\n  --           UTL_FILE.PUT_LINE(utlfilehandleprot, 'Darlehen '||nproverid||'*'||vctyp||'*'||TO_NUMBER(ivcBukrs)||'*'||NVL(nsaldo_lfd,0)||'*'||nt_waehr_id);\r\n              nsaldo_aktuell:=pk_ver2_vertrag_db.FK_HOLE_TRANSW_SALDO_TAGGENAU(rcVertraege.pro_ver_id,rcVertraege.typ,\r\n                                 rcVertraege.buchungskreis,NVL(rcVertraege.saldo_lfd,0),rcVertraege.t_waehr_id,SYSDATE);\r\n\
      \r\n              UPDATE produktverwendungen\r\n                 SET saldo_lfd=nsaldo_aktuell,\r\n                   prog_name = 'Saldenholen'\r\n               WHERE id=rcVertraege.pro_ver_id;\r\n\
      \r\n            END IF;\r\n\
      END LOOP;\r\n\
      END;            "
