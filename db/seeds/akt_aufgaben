- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 2011-01-10 00:00:00
    erfasst: 2010-10-28 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "nach dem Wechsel aus UB-AZn nach Kredit -AZN kommt bei der ersten Änderung (IR-AZn) Fehlermeldung\r\n\
      \r\n\
      * UBs raussuchen\r\n\
      * Beim Wechsel von UB nach Aktiv-Ausziffern, wurde die Berechtung zum Ändern der Zahlungseingänge nicht wieder hergestellt.\r\n\
      * Angepasst und auf IKB3 geschoben\r\n\
      * *abw1s001.fmb*"
    mantis_nummer: "47579"
    aufgabe_id: "3839"
    id: "3822"
    information: |
      SELECT pvw.kd_nr,pvw.lfd_nr,pvw.tranche,cf.offen_betrag

        FROM produktverwendungen pvw

        JOIN cf_zuordnungen cfzu ON (cfzu.pro_ver_id = pvw.id)

        JOIN cashflows cf ON (cf.cftzuo_id = cfzu.id)

      WHERE pvw.buchungskreis = 20 AND pvw.typ = 'UB'

         AND pvw.verwendungs_status IN ('I','Z')

         AND pvw.loeschdatum IS NULL

         AND pvw.extern_kz <> 'J'

         AND cf.la_beleg_nr IS NOT NULL

         AND cf.v_verzg_schl IS NULL

         AND cf.offen_betrag >0

         AND cf.v_buschl_schl <> 998

         AND EXISTS (SELECT ID

                      FROM VW_VER9_CF0_BUSCHL_GRUPPEN v

                     WHERE cf.ID = v.ID

                       AND faellig_az = 0)

      AND NOT EXISTS (SELECT 1

                               FROM ze_zuordnungen zezu

                              WHERE zezu.zlgeing_id = 0

                                AND zezu.ruecknahme_dat IS NULL

                                AND cf.ID = zezu.cashfl_id)

             AND cf.v_buschl_schl NOT IN (1, 6) ;      


