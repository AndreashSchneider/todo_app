      * eingecheckt und auf IKB2 eingestellt.\r\n\
      * pk_bik5_ev_sf.pkb ebenfalls angepasst und geändert auf v_dokst_id \r\n"
    mantis_nummer: "39348"
    aufgabe_id: 
    id: "3256"
    information: "SELECT * FROM ev_druckdaten WHERE entschv_id = 168380 AND name LIKE 'Anlage_%';\r\n\
      \r\n\
      *SELECT anwender_titel, angefordert_dat versionsdatum, server_dateiname anwender_dateiname\r\n FROM ev_batch_auftraege \r\n  WHERE entschv_id = 168380;\r\n   and report_name IN* \r\n\
      \r\n\
      SELECT anwender_titel, versionsdatum, anwender_dateiname\r\n     FROM   dokument_zuordnungen\r\n     WHERE  TABELLE_ID = 168380    -- wg. Performance\r\n     AND    EXISTS (\r\n        SELECT '1'\r\n        FROM   DOKUMENT_STRUKTUREN ST\r\n        WHERE  DOKUMENT_ZUORDNUNGEN.V_DOKST_ID = ST.ID\r\n        AND    ST.kuerzel IN (SELECT   kuerzel\r\n                 FROM     dokument_strukturen\r\n                 WHERE    v_dokst_id IN (\r\n                            SELECT ID\r\n                            FROM   dokument_strukturen\r\n                            WHERE  kuerzel IN\r\n                                     ('UFN'\r\n                                    , 'UFB'\r\n                                    , 'UFA'\r\n                                    , 'UFAA'\r\n                                    , 'UFS'\r\n                                    , 'IFN'\r\n                                    , 'IFB'\r\n                                    , 'IFA'\r\n                                    , 'IFS'\r\n                                     )))\r\n                 \r\n        AND    DOKUMENT_ZUORDNUNGEN.VERSIONSDATUM <= SYSDATE\r\n        AND    DOKUMENT_ZUORDNUNGEN.TABELLE_ID =\r\n                 168380\r\n        AND    (   DOKUMENT_ZUORDNUNGEN.LOESCHDATUM IS NULL\r\n          OR DOKUMENT_ZUORDNUNGEN.LOESCHDATUM > SYSDATE\r\n         ))\r\n     AND    NOT EXISTS (\r\n        SELECT 1\r\n        FROM   DOKUMENT_ZUORDNUNGEN Z2\r\n        WHERE  DOKUMENT_ZUORDNUNGEN.ANWENDER_DATEINAME =\r\n               Z2.ANWENDER_DATEINAME\r\n        AND    Z2.TABELLE_ID = dokument_zuordnungen.TABELLE_ID\r\n        AND    DOKUMENT_ZUORDNUNGEN.V_DOKST_ID = Z2.V_DOKST_ID\r\n        AND    Z2.VERSIONSDATUM >\r\n              DOKUMENT_ZUORDNUNGEN.VERSIONSDATUM\r\n        AND    Z2.VERSIONSDATUM <= SYSDATE\r\n        AND    (   Z2.LOESCHDATUM IS NULL\r\n          OR Z2.LOESCHDATUM > SYSDATE))\r\n UNION      \r\n               -- Die nicht verarbeiteten Batchaufträge müssen mit angezeigt werden,\r\n               -- da die Batchverarbeitung neuerdings vom Report überholt wird !! \r\n                   SELECT anwender_titel, angefordert_dat versionsdatum, server_dateiname anwender_dateiname   \r\n                     FROM ev_batch_auftraege  ba\r\n                     JOIN DOKUMENT_STRUKTUREN ds on (ds.id = ba.V_DOKST_ID )\r\n                    WHERE ba.entschv_id = 168380\r\n                      AND ba.finished_dat IS NULL        \r\n                      AND ds.KUERZEL IN  (SELECT   kuerzel\r\n                 FROM     dokument_strukturen\r\n                 WHERE    v_dokst_id IN (\r\n                            SELECT ID\r\n                            FROM   dokument_strukturen\r\n                            WHERE  kuerzel IN\r\n                                     ('UFN'\r\n                                    , 'UFB'\r\n                                    , 'UFA'\r\n                                    , 'UFAA'\r\n                                    , 'UFS'\r\n                                    , 'IFN'\r\n                                    , 'IFB'\r\n                                    , 'IFA'\r\n                                    , 'IFS'\r\n                                     )))  \r\n\
