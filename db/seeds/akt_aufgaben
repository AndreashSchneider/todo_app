    mantis_nummer: 
    aufgabe_id: 
    id: "3198"
    information: "SELECT e.aend_dat,per.kd_nr,evb.status,e.typ,e.art,e.mkt_votum_vorgang_kz FROM entscheidungsvorlagen e\r\n  JOIN ev_bearbeitungen evb ON (evb.entschv_id = e.id AND aktuell_kz='J')\r\n  JOIN personen per ON per.id = e.person_id\r\n WHERE e.verwendung_kz = 3\r\n   AND evb.status >10\r\n\
      ;\r\n\
      \r\n\
      -- dieses Update auf IKB1 durchgeführt --> 16 Fälle angepasst\r\n\
      UPDATE entscheidungsvorlagen SET verwendung_kz = 1 WHERE \r\n\
      id IN (SELECT e.id FROM entscheidungsvorlagen e\r\n  JOIN ev_bearbeitungen evb ON (evb.entschv_id = e.id AND aktuell_kz='J')\r\n  JOIN personen per ON per.id = e.person_id\r\n WHERE e.verwendung_kz = 3\r\n   AND evb.status >10)\r\n\
      ;\r\n\
      \r\n\
      !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!1letztes Update !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!1\r\n\
      DECLARE \r\n  v_kd_nr  PERSONEN.KD_NR%TYPE ;\r\n  v_typ    entscheidungsvorlagen.typ%TYPE ;\r\n  v_art    entscheidungsvorlagen.art%TYPE ;\r\n  c_count  number;\r\n\
      BEGIN \r\n v_kd_nr:= null;\r\n  c_count:= 0; \r\n  DBMS_OUTPUT.PUT_LINE('Nur für Kurz-EV NEU: ');\r\n  \r\n   FOR c1 IN ( \r\n      SELECT e.id entschv_id, e.aend_dat,e.aend_user,per.kd_nr,evb.status,e.typ,e.art,e.mkt_votum_vorgang_kz,evb.id evb_id FROM entscheidungsvorlagen e\r\n      JOIN ev_bearbeitungen evb ON (evb.entschv_id = e.id AND aktuell_kz='J')\r\n      JOIN personen per ON per.id = e.person_id\r\n     WHERE e.verwendung_kz = 1\r\n       AND trunc(e.aend_dat) = trunc(sysdate-1)\r\n       AND e.typ = 1\r\n       AND e.art = 1\r\n       AND e.aend_user = 'AE'\r\n       AND evb.status >10\r\n       AND evb.status <50 ) \r\n    LOOP\r\n                 \r\n    UPDATE entscheidungsvorlagen\r\n     SET verwendung_kz = 3\r\n     , MKT_VOTUM_VORGANG_KZ = 'J'\r\n     , kurz_ev_kz ='N'\r\n     WHERE ID = c1.ENTSCHV_ID;\r\n     \r\n      update ev_bearbeitungen set STATUS =10 where  aktuell_kz = 'J'  and id= c1.evb_id  ;\r\n\
      \r\n   DBMS_OUTPUT.PUT_LINE('kd_nr: ' || c1.kd_nr || ' TYP= '|| c1.typ ||' ART= '|| c1.art);  \r\n   c_count := c_count + 1 ;\r\n    \r\n   END LOOP;\r\n DBMS_OUTPUT.PUT_LINE('Anzahl EVen: ' || to_char(c_count) );\r\n \r\n END;"
