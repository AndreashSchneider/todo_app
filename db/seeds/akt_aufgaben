- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 2010-10-05 00:00:00
    erfasst: 2010-07-27 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Funktionen zum Zugriff auf die Limitauflagen für Thorsten Pattberg\r\n\
      \r\n\
      pk_Bik5_Utils.fk_Get_Ev_Id_Limit\r\n\
      * *pk_bik5_utils.fk_get_limit_auflage(n_entschv_id)* bleibt als einzige neue übrig !! \r\n\
      fk_get_limit_freigeber(person_id)\r\n\
      fk_get_limit_freigabe_dat(person_id)\r\n\
      * Warum liefert der Select keine Bezeichnung ??? Weil bik5s229 die Texte in ev-Sachverhalte abspeichert und bik5s209 in bezeichnungen !! Wird über die Funktion automatisch gesteuert !!\r\n\
      * *pk_bik5_utils.pka, pkb* auf ikb3, ikb2, eingecheckt"
    mantis_nummer: "45179"
    aufgabe_id: 
    id: "3701"
    information: |-
      DECLARE 

        FUNCTION  fk_Get_Ev_Id_Limit(in_entschv_id entscheidungsvorlagen.id%type)

          RETURN NUMBER IS

          CURSOR crlimit (in_entschv_id entscheidungsvorlagen.id%type) IS

          SELECT limit_betr 

            FROM limite WHERE entschv_id = in_entschv_id;

          n_limit NUMBER;

        BEGIN 

          OPEN crlimit(in_entschv_id);

          FETCH crlimit INTO n_limit;

          RETURN n_limit;

        END;    

        FUNCTION  fk_get_limit_auflage(in_entschv_id entscheidungsvorlagen.id%type)

          RETURN VARCHAR2 IS    

          CURSOR crLi_kuerzel IS 

          SELECT b.id FROM bezeichnungen b 

           JOIN bezeichnungs_arten ba ON (b.v_bezar_id = ba.id AND ba.kuerzel = 'DELI');

          n_limit_kuerzel_id NUMBER;

        BEGIN 

          OPEN crLi_kuerzel;

          FETCH crLi_kuerzel INTO n_limit_kuerzel_id;

          RETURN nvl(pk_bik5_bezeichnungen.fk_get_bez_text(n_limit_kuerzel_id,'E'),'');

        END;    

        FUNCTION  fk_get_limit_freigeber(in_entschv_id entscheidungsvorlagen.id%type)

          RETURN NUMBER IS    

        BEGIN 

          RETURN pk_bik5_utils.fk_get_status_pers_nr(in_entschv_id,'ENT');

        END;    

        FUNCTION  fk_get_limit_freigabe_dat(in_entschv_id entscheidungsvorlagen.id%type)

          RETURN date IS

        BEGIN 

          RETURN pk_bik5_utils.fk_get_Entscheidung_dat(in_entschv_id);

        END;    

      BEGIN 

          dbms_output.put_line(fk_Get_Ev_Id_Limit(174885));

          dbms_output.put_line(fk_get_limit_auflage(174885));

          dbms_output.put_line(fk_get_limit_freigeber(174885));

          dbms_output.put_line(fk_get_limit_freigabe_dat(174885));

      END;

      -- geht auch in einem Select !!

      SELECT pk_bik5_utils.fk_get_Entscheidung_dat(e.id) entschieden_am,

              pk_bik5_utils.fk_get_status_pers_nr(e.id,'ENT')entscheider,

              pk_Bik5_Utils.fk_Get_Ev_Id_Limit(e.person_id) aktuellste_positiv_entsch,

              e.id, bez.id bez_id,

              pk_bik5_bezeichnungen.fk_get_bez_text(bez.id,'E'),li.limit_betr,li.t_waehr_id,bez.bezeichnung

        FROM entscheidungsvorlagen e

        LEFT OUTER JOIN limite li ON (li.entschv_id = e.id)

        LEFT outer JOIN bezeichnungen bez ON (e.id = bez.entschv_id)

        LEFT OUTER JOIN bezeichnungs_arten beza ON (beza.id = bez.v_bezar_id )

        WHERE beza.kuerzel = 'DELI'

        AND e.person_id = 52044

        AND e.id = 174885;

       WHERE pk_Bik5_Utils.fk_Get_Ev_Id_Limit(e.person_id) = 52044;

       SELECT * FROM personen WHERE kd_nr = 142551;
    projekt: Ent.Vorlage
