    mantis_nummer: 
    aufgabe_id: 
    id: "2547"
    information: "-- freigabe_mit_kompetenz\r\n\
      -- Wenn die erste Freigabe durch Kompetenz-Mitarbeiter erfolgt, dann darf die\r\n\
      -- 2. Freigabe auch ohne Kompetenz erfolgen.\r\n  SELECT perprof.v_anw_id\r\n       , ref1.RV_ABBREVIATION STATUS_KUERZEL\r\n       , ref1.rv_meaning\r\n       , ev_bea.anl_dat\r\n       , ev_bea.bearb_dat\r\n       ,prof.profil_name\r\n    FROM         ev_bearbeitungen ev_bea\r\n            -- mit bestimmten Status...\r\n               JOIN\r\n                 cg_ref_codes ref1\r\n               ON (    ref1.RV_LOW_VALUE = ev_bea.status\r\n                   AND ref1.RV_ABBREVIATION = 'FRE'-- dieser Status wird gesucht !!\r\n                   AND ref1.RV_DOMAIN = 'ENTSCHEIDUNGSSTATUS')\r\n             JOIN\r\n               personen_profile perprof\r\n             ON (ev_bea.pers_nr = perprof.person_pers_nr)\r\n           JOIN\r\n           -- ...hat welches Profil \r\n             PROFILE prof\r\n           ON (prof.id = perprof.v_prof_id)\r\n    \r\n   WHERE ev_bea.entschv_id = 146488 -- 14276\r\n         AND perprof.v_anw_id IN (SELECT id\r\n                                    FROM anwendungen\r\n                                   WHERE anw_name = 'BIK')\r\n\
      ORDER BY ev_bea.bearb_dat DESC; \r\n SELECT e.id,per.kd_nr\r\n       , DECODE(\r\n           e.art,\r\n           4, 'Aktionplan',\r\n           3, 'Sori-Waiver',\r\n           2, 'Bestand-Ratingreview',\r\n           1, 'Neu',\r\n           6, 'Tech-Waiver',\r\n           e.art)\r\n           art\r\n       , DECODE(e.typ, 1, 'UF', 2, 'IF', 4, 'SF', e.typ) typ\r\n       , e.*\r\n    FROM   entscheidungsvorlagen e\r\n         JOIN\r\n           personen per\r\n         ON (per.id = e.person_id)\r\n\
      -- Hier Typ und Art auswählen         \r\n   WHERE typ = 4 AND art = 6\r\n\
      -- Hier nur mit relevanten RISA-Einträgen\r\n         AND EXISTS\r\n              (SELECT 1\r\n                 FROM vw_ris0_insolv_ereign risa\r\n                WHERE     risa.person_id = e.person_id\r\n                      AND risa.zustand IS NOT NULL\r\n                      AND risa.status IS NOT NULL\r\n                      AND ADD_MONTHS(TRUNC(risa.status_dat, 'MM'), 18) >\r\n                           TRUNC(SYSDATE, 'MM'))\r\n\
      ORDER BY per.kd_nr;\r\n"
