- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2009-05-28 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: " Deals \"in Bearbeitung\"mit und ohne Eintrag in p_v_deal_daten!! Sabine Volk Deal 264780 hat am 26.05.2009 in der Excelliste kein Obligo und GV. Es gibt für Deals in Bearbeitungen welche mit Eintrag in p_v_deal_daten und manche - wie dieser - haben keinen Eintrag. Warum ?? <b> falsche Abfrage - die Richtige liefert keine Sätze !!! </b>"
    mantis_nummer: 
    aufgabe_id: 
    id: "2525"
    information: |
      -- Deal in Bearbeitung ohne Eintrag in p_v_deal_daten, die schon mal freigegeben wurden !!

      SELECT pvw.kd_nr,pvw.lfd_nr,pvw.anl_dat,pvw.aend_dat,pv.* FROM produktverwendungen pvw 

      LEFT outer  JOIN p_v_deal_daten pv ON (pv.kd_nr = pvw.kd_nr AND pv.lfd_nr =pvw.lfd_nr)                 

      WHERE pv.kd_nr IS NULL

        AND pvw.bearb_status = 'B' AND pvw.typ = 'DEAL'

        AND pvw.loeschdatum IS NULL

        AND pvw.verwendungs_status ='Z'

        AND ur_freigabe_dat IS NOT null; --dieses Datum wird bei der ersten Freigabe gesetzt.

      

      SELECT LOG.bearb_dat, LOG.beschreibung, LOG.alt, LOG.neu, per.kbez

        FROM   vw_dlm0_logging LOG

             JOIN

               personen per

             ON (LOG.pers_nr = per.pers_nr)

       WHERE LOG.kd_nr = 363736 ---264780 

       ORDER BY bearb_dat DESC;

       

       SELECT kd_nr,lfd_nr,aend_dat  FROM produktverwendungen WHERE bearb_status = 'B' AND typ = 'DEAL';

      SELECT *

        FROM p_v_deal_daten pv

       WHERE EXISTS

               (SELECT 1

                  FROM produktverwendungen pvw

                 WHERE     pvw.kd_nr = pv.kd_nr

                       AND pvw.lfd_nr = pv.lfd_nr

                       AND bearb_status = 'B');

                       


    projekt: SF-Monitoring
    heute_bearbeiten_kz: N
    erklaerung: Frank Wunsch fragen
    erledigt_am: 2009-05-29 00:00:00
    reihenfolge: 
    termin: 2009-05-29 00:00:00
