      SELECT vst.typ,vst.kd_nr deal_kd_nr,vst.lfd_nr deal_lfd_nr  \r\n      ,vst.vertrag_id,vb1.vertrag_unter_id         \r\n      ,vb1.vertrag_ueber_id,vb2.vertrag_unter_id        \r\n      ,vst2.kd_nr,vst2.lfd_nr,vst2.tranche,sap.saldo,sap.konten_art,sap.hauptbuch  \r\n  FROM vertrags_staemme vst,       \r\n       v_beziehungen vb1,       \r\n       v_beziehungen vb2,       \r\n       sap_kontosalden sap,       \r\n       vertrags_staemme vst2 \r\n WHERE vst.kd_nr = 281959 AND vst.lfd_nr = 1   AND vst.typ = 'DEAL'   \r\n   AND vb1.vertrag_ueber_id= vst.vertrag_id   \r\n   AND vb1.vertrag_unter_id = vb2.vertrag_ueber_id   \r\n   AND vb1.bez_kuerzel='DEAL'   \r\n   AND vb2.bez_kuerzel='GZIR'   \r\n   AND vb2.vertrag_unter_id = vst2.vertrag_id   \r\n   AND vb2.vertrag_unter_id = sap.vertrag_id   \r\n   AND sap.konten_art IN ('EW', 'RS','ZN')   \r\n   AND TO_DATE('01.12.2008','dd.mm.yyyy') BETWEEN vst.vf_dat AND vst.vt_dat   \r\n   AND TO_DATE('01.12.2008','dd.mm.yyyy') BETWEEN vb1.vf_dat AND vb1.vt_dat   \r\n   AND TO_DATE('01.12.2008','dd.mm.yyyy') BETWEEN vb2.vf_dat AND vb2.vt_dat   \r\n   AND TO_DATE('01.12.2008','dd.mm.yyyy') BETWEEN sap.vf_dat AND sap.vt_dat    \r\n\
      UNION --  Abfrage der EWB auf DWS1 zu einem bestimmten Datum über Deal-> Vertrag\r\n SELECT vst.typ,vst.kd_nr deal_kd_nr,vst.lfd_nr deal_lfd_nr\r\n       ,vst.vertrag_id,vb1.vertrag_unter_id         \r\n       ,vb1.vertrag_ueber_id,NULL        \r\n       ,vst2.kd_nr,vst2.lfd_nr,vst2.tranche,sap.saldo,sap.konten_art,sap.hauptbuch  \r\n  FROM vertrags_staemme vst,       \r\n       v_beziehungen vb1,       \r\n       sap_kontosalden sap,       \r\n       vertrags_staemme vst2 \r\n WHERE vst.kd_nr = 281959 AND vst.lfd_nr = 1   AND vst.typ = 'DEAL'   \r\n   AND vb1.vertrag_ueber_id= vst.vertrag_id   \r\n   AND vb1.vertrag_unter_id=sap.vertrag_id   \r\n   AND vb1.bez_kuerzel='DEAL'   \r\n   AND vb1.vertrag_unter_id = vst2.vertrag_id   \r\n   AND sap.konten_art IN ('EW', 'RS','ZN')   \r\n   AND TO_DATE('01.12.2008','dd.mm.yyyy') BETWEEN vst.vf_dat AND vst.vt_dat   \r\n   AND TO_DATE('01.12.2008','dd.mm.yyyy') BETWEEN vb1.vf_dat AND vb1.vt_dat   \r\n   AND TO_DATE('01.12.2008','dd.mm.yyyy') BETWEEN sap.vf_dat AND sap.vt_dat  "
