- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 2010-10-06 00:00:00
    erfasst: 2010-10-01 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Hallo Herr Schneider, \r\n\
      \r\n\
      * können Sie mir bitte eine Auswertung über Hyperion SF-Portfolio Review ziehen über alle AKQF-Deals:\r\n\
      Deal-Nr. Deal-Name NL VVS AVS last year EBIT(DA) 2009 last twelve months EBIT(DA) Volumen\r\n\
      * per mail geantwortet.\r\n\
      * keine Antwort bis heute, daher geschlossen."
    mantis_nummer: 
    aufgabe_id: 
    id: "3772"
    information: |-
      SELECT kd_nr,lfd_nr,ebitda,datum,totDEbt,avs,org_nl, decode(datum,2010,pk_kis0_sf_monitoring.sf_getdealwertid(id,'GV')) gesch_vol FROM 

      ( SELECT pvw.id,pvw.kd_nr,pvw.lfd_nr,

         sum(monw.wert_ist)/1000 ebitda,

         TO_CHAR(PK_DLM0_BASIS.FK_ERMITTLE_GJ_BEGINN_ZU_DATUM(pvw.gj_beginn_kunde,monw.datum),'YYYY') datum,

         PK_DLM0_FINANCIALS.FK_BS_DEBT_ISTWERT_ERMITTELN(pvw.id,0 , PK_DLM0_BASIS.FK_ERMITTLE_GJ_BEGINN_ZU_DATUM(pvw.gj_beginn_kunde , monw.datum )) / 1000 totDEbt            

         ,per.avs,per.org_nl

        FROM produktverwendungen pvw

      -- hier kommt das Ebitda her  

        LEFT OUTER JOIN dlm_monat_werte monw

                ON (pvw.id = monw.pro_ver_id  

                    AND NOT(monw.wert_ist IS NULL)

                    AND monw.fy_kennzahl_art =2

                   )

        JOIN vw_per0_personen per ON(per.kd_nr = pvw.kd_nr)

      WHERE pvw.typ = 'DEAL'

       --  AND pvw.kd_nr = 136801 

       --  AND lfd_nr = 1  

      --   AND datum = to_date('2009','YYYY')

      GROUP BY pvw.id,pvw.kd_nr,pvw.lfd_nr, TO_CHAR(PK_DLM0_BASIS.FK_ERMITTLE_GJ_BEGINN_ZU_DATUM(pvw.gj_beginn_kunde,monw.datum),'YYYY'),

      PK_DLM0_FINANCIALS.FK_BS_DEBT_ISTWERT_ERMITTELN(pvw.id, 0, PK_DLM0_BASIS.FK_ERMITTLE_GJ_BEGINN_ZU_DATUM( pvw.gj_beginn_kunde, monw.datum))

      ,per.avs,per.org_nl

      ORDER BY pvw.kd_nr,lfd_nr,datum)

      WHERE datum  IN (2009,2010)
    projekt: SF-Monitoring
    heute_bearbeiten_kz: N
    erklaerung: ""
    erledigt_am: 2010-10-06 00:00:00
    reihenfolge: 
    termin: 
  attributes_cache: {}

