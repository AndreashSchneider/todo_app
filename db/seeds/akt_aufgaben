- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2010-01-08 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Wer hat welches Profil hat tacs, transaktionen_dbs?\r\n\
      \r\n\
      # Frage: In welchem Profil ist die Tac xy enthalten?\r\n\
      # Frage: Über welches Profil hat die Person a die Tac b in Anwendung c \r\n\
      Suchen nach Tac, Profil und Ausprägung der Tac"
    mantis_nummer: 
    aufgabe_id: 
    id: "1157"
    information: "--------------------------------------------------------------------------------\r\n\
      -- Frage: In welchem Profil ist die Tac xy enthalten?\r\n\
      --------------------------------------------------------------------------------\r\n\
      SELECT tac.tac_name,prof.profil_name,proftac.funktion_z z,\r\n       proftac.funktion_n n,proftac.funktion_k k,\r\n       proftac.funktion_l l,proftac.funktion_f f,proftac.funktion_s s,tac.kbez\r\n  FROM vw_glb1_transaktionen_dbs tac\r\n  JOIN profile_tacs proftac ON (proftac.v_tacdb_id= tac.id)\r\n  JOIN PROFILE prof ON (prof.id = proftac.v_prof_id)\r\n  JOIN anwendungen anw ON (anw.id = tac.v_anw_id)\r\n WHERE tac.tac_name = '5S101'\r\n    AND anw.anw_name = 'BIK';\r\n\
      \r\n\
      \r\n\
      --------------------------------------------------------------------------------\r\n\
      -- Frage: Über welches Profil hat die Person a die Tac b in Anwendung c \r\n\
      -- Suchen nach Tac, Profil und Ausprägung der Tac\r\n\
      --------------------------------------------------------------------------------\r\n\
      SELECT perprof.person_pers_nr                                                   \r\n     , (SELECT kbez                                                             \r\n          FROM personen per                                                     \r\n         WHERE per.pers_nr = perprof.person_pers_nr)\r\n         Mitarbeiter                                                            \r\n     , tac.tac_name\r\n     , tac.kbez                                                          \r\n     , prof.profil_name                                                         \r\n     , proftac.funktion_z z,\r\n       proftac.funktion_n n,proftac.funktion_k k,proftac.funktion_f f,\r\n       proftac.funktion_l l,proftac.funktion_s s                                                                \r\n  FROM  personen_profile perprof                                                \r\n  JOIN  PROFILE prof  ON (perprof.v_prof_id = prof.ID)                          \r\n  JOIN  profile_tacs proftac  ON (proftac.v_prof_id = prof.ID)                  \r\n  JOIN  vw_glb1_transaktionen_dbs tac ON (proftac.v_tacdb_id = tac.ID)                  \r\n\
      WHERE     perprof.v_anw_id = (SELECT ID FROM anwendungen WHERE anw_name = 'BIK')                                                               \r\n   AND tac.tac_name = '5S308'         \r\n -- AND perprof.person_pers_nr = '210005'                                    \r\n\
      ORDER BY tac_name;                                                       \r\n\
      \r\n\
      \r\n\
      \r\n"
    projekt: KreDa
    heute_bearbeiten_kz: N
    erklaerung: ""
    erledigt_am: 2010-01-08 00:00:00
    reihenfolge: 
    termin: 2010-01-08 00:00:00
  attributes_cache: {}

