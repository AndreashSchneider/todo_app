- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2009-01-22 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Vergleich zwischen rfc und zzglt3t weist für den 21.01. riesige Mengenunterschiede auf. Heute 554 gegenuüber 146. Ein Stichprobentest (ca 30-40Proben) zeigt, dass es sich um Buchungen handelt, die in Summe wieder 0 ergeben !"
    mantis_nummer: 
    aufgabe_id: 
    id: "2231"
    information: |-
      SELECT zdebi1.mcod1, zzglt3t.racct, rbukrs, Tsl08 aenderung

        FROM sapr3.zzglt3t@sap zzglt3t JOIN sapr3.zdebi1@sap zdebi1

             ON(zdebi1.kunnr = zzglt3t.racct)

       WHERE ryear = '2008'

         AND rclnt = '100'

         AND rldnr = 'AD'

         AND rvers = '001'

         AND Tsl08 <> 0

         AND rpmax = 304

         AND SUBSTR(zdebi1.mcod1, 1, 2) IN('KT', 'DK', 'U1', 'LU', 'LK', 'BK')

         AND bukrs <> '0070';

         

       SELECT ikbkey, bukrs, id,anldat,verarbdat

             FROM rfc@sap

             WHERE tabname='KNC1'

             AND SUBSTR(ikbkey,1,2) IN ('KT','DK','U1','LU','LK','BK')

             AND TRUNC(anlDat)=TRUNC(SYSDATE-1)

         --    AND trunc(VERARBDAT) = trunc(SYSDATE-1);

             AND bukrs  <> '0070';
