      -- Die Produktverwendung wurde am 28.02 um 1:02 von pk_kis1_pvw_upd geändert. \r\n\
      SELECT * FROM produktverwendungen WHERE id = 501993;\r\n\
      pk_kis1_pvw_upd;\r\n\
