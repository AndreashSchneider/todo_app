      # aufgabe.rb einfügen\r\n\
      def self.search(search, page)\r\n    paginate :per_page => 5, :page => page,\r\n           :conditions => ['aufgabe like ?', \"%#{search}%\"], :order => 'aufgabe'\r\n end\r\n\
