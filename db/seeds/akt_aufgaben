- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2009-09-23 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: " adhoc Liste für Herrn Mantsch\r\n\
      \r\n\
      * bräuchte einmalig folgende Auswertung bzgl. EVs:\r\n\
      - alle entschiedenen (genehmigt, genehmigt mit Auflagen, abgelehnt) SoRis (SON) seit dem 01.04.07\r\n\
      \r\n\
      NL\r\n\
      AVS\r\n\
      Kd-Nr.\r\n\
      *Rating*\r\n\
      EV-Art\r\n\
      Kompetenz\r\n\
      Entscheidungsgegenstand\r\n\
      E-Vorschlag - Oberbegriff (s. Screenshot; wenn mehrere sind, bräuchte ich alle)\r\n\
      E-Vorschlag - Entscheidungsgegenstände (s. Screenshot; wenn mehrere sind, bräuchte ich alle)\r\n\
      Datum der Erstellung\r\n\
      Datum der Entscheidung"
    mantis_nummer: 
    aufgabe_id: 
    id: "2901"
    information: "SELECT\r\n   nl,            \r\n   oe.org_nr avs,\r\n   kd_nr,        \r\n    --pk_bik5_entscheidungsvorlage.fk_get_element_number (\r\n   --     pk_bik5_entscheidungsvorlage.fk_get_group_id(evdruda.id , 'Angaben zum Kreditnehmer'),\r\n   --                     'RATING')                -- Rating\r\n   ev_art,             \r\n   komp,         \r\n   kbez entscheidungsgegenstand,       \r\n   REF1.RV_MEANING Oberbegriff,      \r\n   EVG.KBEZ Entscheidungsgegenstände,\r\n   ers_dat,       -- Datum der Erstellung\r\n   entsch_vom_dat-- Datum der Entscheidung\r\n\
      FROM vw_bik5_ev_info ev_info\r\n\
      JOIN ev_druckdaten evdruda ON (ev_info.ev_id = evdruda.entschv_id AND evdruda.evdruda_id is NULL )\r\n\
      JOIN entscheidungsvorlagen e ON (ev_info.ev_id = e.id)\r\n\
      JOIN org_einheiten oe ON (oe.id = e.v_oeinh_id)\r\n\
      LEFT OUTER JOIN ev_ggs_zuordnungen evggsz ON (evggsz.entschv_id =e.id )\r\n\
      LEFT OUTER JOIN vw_glb1_ev_gegenstaende EVG ON (evg.id = evggsz.v_evgeg_id)\r\n\
      LEFT OUTER JOIN CG_REF_CODES REF1 ON ( TO_NUMBER(EVG.OBERBEGRIFF) = TO_NUMBER(REF1.RV_LOW_VALUE)     \r\n   AND REF1.RV_DOMAIN = 'EV_OBERBEGRIFF')\r\n\
      WHERE entsch_vom_dat between to_date('01.04.2007','dd.mm.yyyy') and trunc(sysdate)\r\n\
      AND status = 50 \r\n\
      AND ev_art='SON'\r\n\
      ORDER BY e.id,nl,pcs,kd_nr;"
    projekt: Ent.Vorlage
    heute_bearbeiten_kz: N
    erklaerung: ""
    erledigt_am: 2009-09-24 00:00:00
    reihenfolge: 
    termin: 2009-09-24 00:00:00
  attributes_cache: {}

