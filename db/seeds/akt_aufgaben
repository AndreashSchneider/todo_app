      SELECT PK_VER2_VERTRAG_DB.FK_TAG_IM_GJAHR(20,trunc(sysdate-3)) FROM dual;\r\n\
      \r\n\
      -- (28.02) i_rpmax  -> 336, divrest ->14 (letzter Tag der Periode) \r\n\
      SELECT (CEIL(334 / 16) * 16) i_rpmax,MOD(334, 16)letzter_tag_periode FROM dual;\r\n\
      -- (02.03) i_rpmax  -> 352, divrest -> 1\r\n\
      SELECT (CEIL(337 / 16) * 16) i_rpmax,MOD(337, 16)letzter_tag_periode FROM dual;\r\n\
      \r\n\
      --------------------------------------------------------------------------------\r\n\
      -- die Abfrage für die geänderten Konten ermitteln\r\n\
      --------------------------------------------------------------------------------\r\n\
      DECLARE\r\n\
      vcselect         VARCHAR2(2000);\r\n  i_nbuchungskreis produktverwendungen.buchungskreis%TYPE:=20;\r\n  ivchausw_transw_kz    VARCHAR2(1):='T';\r\n  nloop number(2):=14; -- Divrest (Achtung ist Divrest=0, dann 16!!\r\n\
      BEGIN\r\n\
      vcselect :=\r\n      ' SELECT zdebi1.mcod1,zzglt3t.racct,rbukrs,'\r\n   ||LTRIM(ivchausw_transw_kz)\r\n   ||'sl'\r\n   || LTRIM(TO_CHAR(nloop, '09'))|| ' aenderung'\r\n   || '   FROM sapr3.zzglt3t@sap zzglt3t '\r\n   || '   JOIN sapr3.zdebi1@sap zdebi1 ON(zdebi1.kunnr = zzglt3t.racct)  '\r\n   || ' WHERE ryear  = :i_vcGjahr  '\r\n   || ' AND rclnt  = ''100'''\r\n   || ' AND rldnr  = ''AD'''  -- AD= Belegdatum AP wäre Posting (Buchungsdatum)\r\n   || ' AND rvers  = ''001'''\r\n   -- tslx, welches gefüllt sein muss, zusammen bauen !!\r\n   || ' AND  '||LTRIM(ivchausw_transw_kz)||'sl'|| LTRIM(TO_CHAR(nloop, '09'))||' <>0 '\r\n   || ' AND  rpmax = :i_nRpmax '\r\n   || ' AND SUBSTR(zdebi1.mcod1,1,2) IN (''KT'',''DK'',''U1'',''LU'',''LK'',''BK'')';\r\n  \r\n   IF i_nbuchungskreis <>70 THEN\r\n     vcselect:= vcselect||' AND bukrs <> ''0070''';\r\n   ELSE\r\n     vcselect:= vcselect||' AND bukrs = ''0070''';\r\n   END IF;\r\n  dbms_output.put_line(vcselect);\r\n\
      END;\r\n\
      \r\n\
      \r\n\
      -- Abfrage auf geänderte Konten (BK=20) für den 28.02.2010  --> liefert 348 Konten\r\n\
      \r\n\
      SELECT zdebi1.mcod1, zzglt3t.racct, rbukrs, Tsl14 aenderung\r\n  FROM   sapr3.zzglt3t@sap zzglt3t\r\n       JOIN\r\n         sapr3.zdebi1@sap zdebi1\r\n       ON (zdebi1.kunnr = zzglt3t.racct)\r\n\
      WHERE     ryear = 2009\r\n       AND rclnt = '100'\r\n       AND rldnr = 'AD'\r\n       AND rvers = '001'\r\n       AND Tsl14 <> 0\r\n       AND rpmax = 336 --:i_nRpmax\r\n       AND SUBSTR(zdebi1.mcod1, 1, 2) IN ('KT', 'DK', 'U1', 'LU', 'LK', 'BK')\r\n       AND bukrs <> '0070';\r\n      \r\n\
      -- Bespiel KT1883610050000 -> KT 188361 005 0000 hat eine Änderung von -56,08\r\n\
      -- Cashflow über den Betrag am 28.02 ist auch vorhanden\r\n\
      \r\n\
      --------------------------------------------------------------------------------  \r\n\
      -- FK_SALDO_FUELLEN wird für die geänderten Konten aufgerufen\r\n\
      --------------------------------------------------------------------------------\r\n  FK_SALDO_FUELLEN;\r\n  -- KreDa- Daten holen, die für den Saldoaufruf benötigt werden\r\n         SELECT id, saldo_lfd, t_waehr_id,typ,extern_kz,aend_dat,prog_name\r\n           FROM produktverwendungen\r\n          WHERE kd_nr     = 188361\r\n         AND   lfd_nr    = 005\r\n         AND   tranche   = 0\r\n         AND   buchungskreis = 20\r\n         AND   verwendungs_status='I'\r\n\
      --         AND   extern_kz='N'\r\n         AND   typ IN ('IR','BRG')\r\n         AND   loeschdatum IS NULL;\r\n        \r\n\
      -- und den Saldo mit den zuvor ermittelten Parametern holen !!        \r\n\
      SELECT  PK_VER2_VERTRAG_DB.FK_HOLE_TRANSW_SALDO_TAGGENAU(1648238,'IR',\r\n        TO_NUMBER('20'),NVL(8422.18,0),35,SYSDATE)\r\n  FROM dual; \r\n\
      --------------------------------------------------------------------------------  \r\n -- mit einem falschen Saldo testen KreDa 83213,23\r\n -- 6452-8-0 im BK=20 KT0064520080000\r\n\
      -------------------------------------------------------------------------------- \r\n   -- KreDa- Daten holen, die für den Saldoaufruf benötigt werden\r\n         SELECT id, saldo_lfd, t_waehr_id,typ,extern_kz FROM produktverwendungen\r\n         WHERE kd_nr     = 6452\r\n         AND   lfd_nr    = 8\r\n         AND   tranche   = 0\r\n         AND   buchungskreis = 20\r\n         AND   verwendungs_status='I'\r\n\
