      e_next_deal         EXCEPTION;

      

       -- Tabelle der Deals

       TYPE r_deal  IS RECORD(

                           kd_nr  NUMBER(8),

                           lfd_nr NUMBER(3) := 1);

                           

       TYPE t_kd_nr IS TABLE OF r_deal

               INDEX BY PLS_INTEGER;

      

       v_r_deal             r_deal;

       v_t_kd_nr            t_kd_nr;

      

       

      BEGIN

