    mantis_nummer: 
    aufgabe_id: 
    id: "3909"
    information: "-- Berechung des Obligos auf der Reiterkarte Overview, was lt. Sabine Volk, Astrid Werr und Frau Voß-Mengeringhaus falsch ist\r\n\
      SELECT pk_kis0_sf_monitoring.sf_getdealwertid(1313154,'OBL')OBL,\r\n       pk_kis0_sf_monitoring.sf_getdealwertid(1313154,'SYUB')SYUB,\r\n       pk_kis0_sf_monitoring.sf_getdealwertid(1313154,'OBL')\r\n       -pk_kis0_sf_monitoring.sf_getdealwertid(1313154,'SYUB') SF_OBLIGO\r\n\
      \r\n\
      FROM dual;\r\n\
      \r\n\
      \r\n\
      -- Berechung des SF-Obligos auf der Reiterkarte Fazilitäten    \r\n   SELECT SUM (obligo_abzgl_bue)\r\n      FROM   ev_ver_fazilitaeten\r\n      WHERE  entschv_id = 187140\r\n        AND  herkunft   < 3\r\n     -- ORDER  BY sort_nr    --> 756.363,61\r\n     \r\n\
      -- wie werden die Fazilitäten gefüllt?\r\n\
      -- in pk_bik5_deal_werte\r\n\
      -- über die Prozedur pr_vertr_fazi_init\r\n     \r\n\
      -- fk_get_sf_obligo_hwae liefert hier das Oblig abzüglich Bürgschafen, dass die\r\n\
      -- Prozedur weitergibt.\r\n     \r\n\
      -- geholt aus pk_dlm0_krnehm.fk_obl_redu_relevant(vw.pvw) aus \r\n       SELECT pk_dlm0_krnehm.fk_obl_redu_relevant(vw.pvw)\r\n         FROM   vw_dlm0_vertraege vw\r\n          WHERE  vw.pro_ver_id = i_pro_ver_id;\r\n         \r\n\
      FUNCTION fk_get_sf_obligo_hwae(i_pro_ver_id IN PRODUKTVERWENDUNGEN.ID%TYPE)\r\n   RETURN NUMBER\r\n   IS\r\n      n_return        NUMBER;\r\n      v_redu_relevant VARCHAR2(1);\r\n   BEGIN\r\n      n_return := fk_get_pvw_obligo_hwae(i_pro_ver_id);\r\n      BEGIN\r\n         SELECT pk_dlm0_krnehm.fk_obl_redu_relevant(vw.pvw)\r\n         INTO   v_redu_relevant\r\n         FROM   vw_dlm0_vertraege vw\r\n         WHERE  vw.pro_ver_id = i_pro_ver_id;\r\n\
      \r\n         IF v_redu_relevant = 'J'\r\n         THEN\r\n            n_return := n_return - pk_sid0_synthetische_ubs.fk_get_sid_entlastung(i_pro_ver_id);\r\n         END IF;\r\n      EXCEPTION\r\n      WHEN others\r\n      THEN\r\n         NULL;\r\n      END;\r\n\
      \r\n      RETURN n_return;\r\n   END;"
    projekt: Ent.Vorlage
    heute_bearbeiten_kz: N
    erklaerung: ""
    erledigt_am: 2011-01-17 00:00:00
    reihenfolge: 
    termin: 
