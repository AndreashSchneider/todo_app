    information: |
      FUNCTION FK_HAUPTBUCHAENDERUNG(i_nPvwId IN NUMBER, i_nHBuchAlt IN NUMBER, 

                                     i_nHBuchNeu IN NUMBER, i_nUbTyp IN NUMBER DEFAULT NULL) RETURN BOOLEAN IS

        vKonto      pk_glb0_konten.tKonto;  

        vMessage    pk_glb0_konten.tMessage;

        bok         BOOLEAN;

        nret        NUMBER;

      BEGIN  

        vKonto := pk_glb0_konten.fk_get_name_vertrag_id(i_nPvwId);

                                        

        bOk := PK_GLB0_KONTEN.fk_konto_aendern_hauptbuch(vKonto,

                                                         i_nHBuchAlt,

                                                         i_nHBuchNeu,

                                                         i_nUbTyp,

                                                         vMessage);

          

        IF vMessage.nNr IS NOT NULL THEN

          nRet := PK_STD_NACHRICHT.FK_GET_NACHRICHT_BUTTON('IKB',

                                                           vMessage.nNr,

                                                           0,

                                                           vMessage.vcPar1,

                                                           vMessage.vcPar2,

                                                           vMessage.vcPar3);

        END IF;

        IF NOT bok THEN

          RETURN FALSE;

        ELSE

          RETURN TRUE;

        END IF; 

      END;


