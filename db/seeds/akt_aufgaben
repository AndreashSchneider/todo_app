    aufgabe: " Verwendungskürzel vw_bik6_bb_text die anpassen \r\n\
      \r\n\
      gemäß Konzept\r\n\
      \r\n\
      \"geaenderte View unter  X:DS/de/sql\" und eingecheckt"
    mantis_nummer: 
    aufgabe_id: 
    id: "2637"
    information: |-
      CREATE OR REPLACE FORCE VIEW vw_bik6_bb_text (

         v_bezar_id,

         kuerzel,

         kbez,

         sort_nr )

      AS

      SELECT b.id,b.kuerzel,b.kbez

      ,decode(b.kuerzel,'BESZ',1, 

                        'GGST',2, 

                        'MWIS',3, 

                        'MBA', 4, 

                        'FIZW',5, 

                        'MFIN',6, 

                        'MSIK',7, 

                        'MLIQ',8, 

                        'MSOR',9, 

                        'NOVE',10,

                        'MAVE',11,

                        'MENS',12,

                        99) SORT_NR

      FROM vw_glb1_bezeichnungs_arten b

      WHERE b.kuerzel IN ('BESZ','GGST','MWIS','MBA','FIZW','MFIN',

      'MSIK','MLIQ','MSOR','NOVE','MAVE','MENS')

      /
