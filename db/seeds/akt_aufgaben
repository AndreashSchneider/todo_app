- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2009-02-20 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Saldendifferenzen zwischen SAP und KreDa bei Darlehen. Nach langer Suche stellt sich heraus, dass im Aufruf der Saldenermittlung in pk_ver6_sap für Darlehen ein sysdate-1 versteckt war. Daher wurde immer der Saldo vom Vortag geholt. Die untenstehenden 13 Fälle habe ich korrigiert und eine Mail an Herrn Büttner, Wildenberg gesandt."
    mantis_nummer: 
    aufgabe_id: 
    id: "2300"
    information: |-
      -- Skript zum Anpassen 

      DECLARE

        CURSOR crpvw IS

          SELECT *

            FROM produktverwendungen

           where verwendungs_status = 'I'

           AND ((produkt_nr =202542 and lfd_nr = 0004 and tranche = 0415) or

      (produkt_nr =202542 and lfd_nr = 0004 and tranche = 0416) or

      (produkt_nr =100012 and lfd_nr = 0402 and tranche = 0000) or

      (produkt_nr =100016 and lfd_nr = 1058 and tranche = 0000) or

      (produkt_nr =202815 and lfd_nr = 0001 and tranche = 0408) or

      (produkt_nr =202815 and lfd_nr = 0001 and tranche = 0409) or

      (produkt_nr =202830 and lfd_nr = 0001 and tranche = 0345) or

      (produkt_nr =202830 and lfd_nr = 0001 and tranche = 0346) or

      (produkt_nr =202910 and lfd_nr = 0001 and tranche = 0282) or

      (produkt_nr =202910 and lfd_nr = 0001 and tranche = 0283) or

      (produkt_nr =200963 and lfd_nr = 0027 and tranche = 0032) or

      (produkt_nr =200329 and lfd_nr = 0075 and tranche = 0155) or

      (produkt_nr =200329 and lfd_nr = 0075 and tranche = 0156));

      

        heute NUMBER;

        gestern NUMBER;

        vorgestern NUMBER;

      BEGIN

        FOR rec IN crpvw LOOP

          heute :=

            pk_ver2_vertrag_db.fk_hole_transw_saldo_taggenau(rec.ID

                                                           , rec.typ

                                                           , rec.buchungskreis

                                                           , rec.saldo_lfd

                                                           , rec.t_waehr_id

                                                           , TRUNC(SYSDATE));

          gestern :=

            pk_ver2_vertrag_db.fk_hole_transw_saldo_taggenau(rec.ID

                                                           , rec.typ

                                                           , rec.buchungskreis

                                                           , rec.saldo_lfd

                                                           , rec.t_waehr_id

                                                           , TRUNC(SYSDATE-1));

          vorgestern :=

            pk_ver2_vertrag_db.fk_hole_transw_saldo_taggenau(rec.ID

                                                           , rec.typ

                                                           , rec.buchungskreis

                                                           , rec.saldo_lfd

                                                           , rec.t_waehr_id

                                                           , TRUNC(SYSDATE-2));          

      

      dbms_output.put_line(rec.produkt_nr||'*'||rec.lfd_nr||'*'||rec.tranche

                           ||' heute: '||heute||' gestern: '||gestern||' vorgestern: '||vorgestern

                           ||' saldo_lfd: '||rec.saldo_lfd);                                                                                               

        UPDATE produktverwendungen 

        SET saldo_lfd = heute,

        prog_name = 'pk_ver6_sap1'

        WHERE id = rec.id;

        END LOOP;

      END;

      

      COMMIT;
    projekt: KreDa
    heute_bearbeiten_kz: N
    erklaerung: Abends nochmal alle Darlehen mit prog_name pk_ver6_sap korrigiert.
    erledigt_am: 2009-02-20 00:00:00
    reihenfolge: 
    termin: 2009-02-20 00:00:00
  attributes_cache: {}

