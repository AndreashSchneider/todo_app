- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2008-11-24 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "ABNAHME: pk_ver6_sap_abnahme Testen. Funktioniert erst, wenn die pk_ver2_vertrag_db geändert wurde, da der Zugriff über die zzglt3t realisiert wird."
    mantis_nummer: 
    aufgabe_id: 
    id: "2095"
    information: |
      DECLARE

      BEGIN

      

      -- Now call the stored program

        FOR i in 1..20 LOOP 

        ikbobj.pk_ver6_sap_abnahme.pr_geaenderte_sap_konten(TRUNC(SYSDATE-i),20);    

        ikbobj.pk_ver6_sap_abnahme.pr_geaenderte_sap_konten(TRUNC(SYSDATE-i),70);

        END LOOP;

      EXCEPTION

      WHEN OTHERS THEN

        dbms_output.put_line(SubStr('Error '||TO_CHAR(SQLCODE)||': '||SQLERRM, 1, 255));

        dbms_output.put_line(dbms_utility.format_error_backtrace);

      RAISE;

      END;

      

      -- 25-11-2008 ist der 307. Tag im GJ --> daher inrpmax 320 und divrest = 3 im GJ: 2007 Whrg. 

      SELECT zzglt3t.racct,zdebi1.mcod1, Tslvt, Tsl01, Tsl02, Tsl03, Tsl04, Tsl05, Tsl06, Tsl07

           , Tsl08, Tsl09, Tsl10, Tsl11, Tsl12, Tsl13, Tsl14, Tsl15, Tsl16

        FROM sapr3.zzglt3t@sap zzglt3t 

        JOIN sapr3.zdebi1@sap zdebi1 ON(zdebi1.kunnr = zzglt3t.racct)

       WHERE ryear = 2008  

         AND rclnt = '100'

         AND rldnr = 'AD'

         AND rvers = '001'

       --  AND rpmax = 240

       --  AND tsl03 > 0

         AND racct = '0001084895'

      -- AND rtcur  = :vcwaehrcode

      -- AND rbukrs = :i_vcBukrs

         AND SUBSTR(zdebi1.mcod1,1,2) IN ('KT','DK','U1','LU','LK','BK');

       SELECT *

       --( Tslvt+ Tsl01+ Tsl02+ Tsl03+ Tsl04+ Tsl05+ Tsl06+ Tsl07+ Tsl08+ Tsl09+ Tsl10+ Tsl11+ Tsl12+ Tsl13+ Tsl14+ Tsl15+ Tsl16) summe_ganzes_Jahr

        FROM sapr3.zzglt3t@sap zzglt3t 

        JOIN sapr3.zdebi1@sap zdebi1 ON(zdebi1.kunnr = zzglt3t.racct)

       WHERE ryear = 2008  

         AND rclnt = '100'

         AND rldnr = 'AP'

         AND rvers = '001'

       --  AND rpmax = 240

       --  AND tsl03 > 0

         AND racct = '0001256369'

      -- AND rtcur  = :vcwaehrcode

      -- AND rbukrs = :i_vcBukrs

         AND SUBSTR(zdebi1.mcod1,1,2) IN ('KT','DK','U1','LU','LK','BK')

        ;

      

      SELECT ID, aend_dat, saldo_lfd, produkt_nr, kd_nr, lfd_nr, tranche

           , pk_ver2_vertrag_db.FK_HOLE_TRANSW_SALDO_TAGGENAU

                                                            (ID

                                                           , typ

                                                           , TO_NUMBER(buchungskreis)

                                                           , NVL(saldo_lfd, 0)

                                                           , t_waehr_id

                                                           , SYSDATE)

                                                           

        FROM produktverwendungen

       WHERE prog_name = 'pk_ver6_sap';


    projekt: Sap-Schnit.
    heute_bearbeiten_kz: N
    erklaerung: ""
    erledigt_am: 2008-11-24 00:00:00
    reihenfolge: 
    termin: 2008-11-24 00:00:00
  attributes_cache: {}

