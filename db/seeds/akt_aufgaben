      \r\n  endif.\r\n\
      *\r\n\
      * Select für KT Konten\r\n  if i_kontonummer+0(2) = 'KT'.\r\n    move i_kontonummer+8(3) to l_n3.\r\n    move i_kontonummer+11(4) to t_nr.\r\n    EXEC SQL.\r\n      SELECT\r\n        saldo_lfd,\r\n        hauptbuch ,\r\n        to_char(loeschdatum ,'YYYYMMDD') ,\r\n        to_char(auslauf_dat ,'YYYYMMDD') ,\r\n        id\r\n      INTO  :e_saldo,\r\n            :e_hauptbuch ,\r\n            :e_loedatum ,\r\n            :e_ausldatum ,\r\n            :e_id\r\n      FROM produktverwendungen@ikbora pvw\r\n\
      \r\n      WHERE typ = 'IR'\r\n      AND   buchungskreis = :w_bukrs\r\n      AND kd_nr = :p_nr\r\n      AND lfd_nr = :l_n3\r\n      AND tranche = :t_nr\r\n      AND verwendungs_status = 'I'\r\n      ORDER BY loeschdatum DESC\r\n    ENDEXEC.\r\n    if i_orkund eq 'X'.\r\n      EXEC SQL.\r\n        SELECT\r\n               konto_nr\r\n        INTO\r\n              :e_kunnr\r\n        FROM\r\n             sap_konten@ikbora sapkto\r\n        WHERE sapkto.pro_ver_id = :e_id\r\n      ENDEXEC.\r\n    endif.\r\n\
      \r\n  endif.\r\n\
      * Select für LK Konten\r\n  if i_kontonummer+0(2) = 'LK'.\r\n    move i_kontonummer+8(3) to l_n3.\r\n    move i_kontonummer+11(4) to t_nr.\r\n    EXEC SQL.\r\n      SELECT\r\n        saldo_lfd,\r\n        hauptbuch ,\r\n        to_char(loeschdatum ,'YYYYMMDD') ,\r\n        to_char(auslauf_dat ,'YYYYMMDD') ,\r\n        id ,\r\n        rueckst_tilg\r\n      INTO  :e_saldo,\r\n            :e_hauptbuch ,\r\n            :e_loedatum ,\r\n            :e_ausldatum ,\r\n            :e_id,\r\n            :e_tilrue\r\n      FROM produktverwendungen@ikbora pvw\r\n\
      \r\n      WHERE typ = 'IR'\r\n      AND   buchungskreis = :w_bukrs\r\n      AND kd_nr = :p_nr\r\n      AND lfd_nr = :l_n3\r\n      AND tranche = :t_nr\r\n      AND verwendungs_status = 'I'\r\n      ORDER BY loeschdatum DESC\r\n    ENDEXEC.\r\n    if i_orkund eq 'X'.\r\n      EXEC SQL.\r\n        SELECT\r\n               konto_nr\r\n        INTO\r\n              :e_kunnr\r\n        FROM\r\n             sap_konten@ikbora sapkto\r\n        WHERE sapkto.pro_ver_id = :e_id\r\n      ENDEXEC.\r\n    endif.\r\n\
      \r\n  endif.\r\n\
      \r\n\
      *\r\n\
      * Select für BK Konten\r\n  if i_kontonummer+0(2) = 'BK'.\r\n    if i_kontonummer+15(1) = ' '.\r\n      move i_kontonummer+8(3) to l_n3.\r\n      move i_kontonummer+11(4) to t_nr.\r\n      EXEC SQL.\r\n        SELECT\r\n          saldo_lfd,\r\n          hauptbuch ,\r\n          to_char(loeschdatum ,'YYYYMMDD') ,\r\n          to_char(auslauf_dat ,'YYYYMMDD'),\r\n          id\r\n      INTO  :e_saldo,\r\n            :e_hauptbuch ,\r\n            :e_loedatum ,\r\n            :e_ausldatum ,\r\n            :e_id\r\n      FROM produktverwendungen@ikbora pvw\r\n\
      \r\n      WHERE typ = 'BRG'\r\n      AND   buchungskreis = :w_bukrs\r\n      AND   kd_nr = :p_nr\r\n      AND   lfd_nr = :l_n3\r\n      AND   tranche = :t_nr\r\n      AND   verwendungs_status = 'I'\r\n\
      *     AND loeschdatum IS null\r\n      ORDER BY loeschdatum DESC\r\n      ENDEXEC.\r\n      if i_orkund eq 'X'.\r\n        EXEC SQL.\r\n          SELECT\r\n                 konto_nr\r\n          INTO\r\n                :e_kunnr\r\n          FROM\r\n               sap_konten@ikbora sapkto\r\n          WHERE sapkto.pro_ver_id = :e_id\r\n        ENDEXEC.\r\n      endif.\r\n\
      \r\n    endif.\r\n  endif.\r\n\
      *\r\n\
      * select für bk konten    unterbeteiligungen\r\n  if i_kontonummer+0(2) = 'BK'.\r\n    if i_kontonummer+15(1) <> ' ' .\r\n      move i_kontonummer+8(3) to l_n3.\r\n      move i_kontonummer+11(4) to t_nr.\r\n      move i_kontonummer+15(1) to u_n1.\r\n      EXEC SQL.\r\n        SELECT\r\n          ub.saldo_lfd,\r\n          ub.hauptbuch ,\r\n          to_char(ub.loeschdatum ,'YYYYMMDD') ,\r\n          to_char(ub.auslauf_dat ,'YYYYMMDD'),\r\n          ub.id\r\n\
      *         konto_nr\r\n   INTO  :e_saldo,\r\n         :e_hauptbuch ,\r\n         :e_loedatum ,\r\n         :e_ausldatum ,\r\n         :e_id\r\n\
      \r\n     FROM produktverwendungen@ikbora pvw\r\n    ,pvw_beziehungen@ikbora pvwbe\r\n    ,pvw_beziehungstypen@ikbora pvwbety\r\n    ,produktverwendungen@ikbora ub\r\n\
      *   ,sap_konten@ikbora sapkto                    \"KL\r\n    WHERE pvw.id = pvwbe.pro_ver2_id\r\n      AND   PVW.BUCHUNGSKREIS = :W_bukrs\r\n      AND pvwbe.v_pvwty_id = pvwbety.id\r\n      AND ub.id = pvwbe.pro_ver1_id\r\n\
      *    AND UPPER(PVWBETY.KUERZEL) = 'KRUB'\r\n       AND pvwbety.kuerzel = 'KRUB'\r\n      AND pvw.kd_nr = :p_nr\r\n      AND pvw.lfd_nr = :l_n3\r\n      AND pvw.tranche = :t_nr\r\n      AND pvw.typ     = 'BRG'\r\n      AND pvw.verwendungs_status = 'I'\r\n      AND ub.lfd_nr = :u_n1\r\n      ORDER BY ub.loeschdatum DESC\r\n\
      *     AND ub.id=sapkto.pro_ver_id                \"KL\r\n      ENDEXEC.\r\n\
      \r\n      if i_orkund eq 'X'.\r\n        EXEC SQL.\r\n          SELECT\r\n                 konto_nr\r\n          INTO\r\n                :e_kunnr\r\n          FROM\r\n               sap_konten@ikbora sapkto\r\n          WHERE sapkto.pro_ver_id = :e_id\r\n        ENDEXEC.\r\n      endif.\r\n\
      \r\n    endif.\r\n  endif.\r\n\
      *\r\n\
      * Select für UB Konten\r\n  if i_kontonummer+0(1) = 'U'.\r\n    move i_kontonummer+8(3) to l_n3.\r\n    move i_kontonummer+11(4) to t_nr.\r\n\
      *** MA,09.01.2009 ***\r\n\
      * Durch die Umstellung der UB-Nummern auf 2 Stellen,\r\n\
      * müssen hier auch die 2 Stellen übergeben werden.\r\n    move i_kontonummer+15(2) to u_n1.\r\n    EXEC SQL.\r\n      SELECT\r\n             ub.saldo_lfd,\r\n             ub.hauptbuch ,\r\n             to_char(ub.loeschdatum ,'YYYYMMDD') ,\r\n             to_char(ub.auslauf_dat ,'YYYYMMDD'),\r\n             ub.id\r\n\
      *            konto_nr                                   \"KL\r\n      INTO  :e_saldo,\r\n            :e_hauptbuch ,\r\n            :e_loedatum ,\r\n            :e_ausldatum ,\r\n            :e_id\r\n\
      \r\n        FROM produktverwendungen@ikbora pvw\r\n       ,pvw_beziehungen@ikbora pvwbe\r\n       ,pvw_beziehungstypen@ikbora pvwbety\r\n       ,produktverwendungen@ikbora ub\r\n\
      *       ,sap_konten@ikbora sapkto                        \"KL\r\n       WHERE pvw.id = pvwbe.pro_ver2_id\r\n         AND   PVW.BUCHUNGSKREIS = :w_bukrs\r\n         AND pvwbe.v_pvwty_id = pvwbety.id\r\n         AND ub.id = pvwbe.pro_ver1_id\r\n\
      *         AND UPPER(PVWBETY.KUERZEL) = 'KRUB'\r\n         AND pvwbety.kuerzel = 'KRUB'\r\n         AND pvw.kd_nr = :p_nr\r\n         AND pvw.lfd_nr = :l_n3\r\n         AND pvw.tranche = :t_nr\r\n         AND ub.lfd_nr = :u_n1\r\n         ORDER BY ub.loeschdatum DESC\r\n\
      *        AND ub.id=sapkto.pro_ver_id                     \"KL\r\n    ENDEXEC.\r\n\
      \r\n      if i_orkund eq 'X'.\r\n        EXEC SQL.\r\n          SELECT\r\n                 konto_nr\r\n          INTO\r\n                :e_kunnr\r\n          FROM\r\n               sap_konten@ikbora sapkto\r\n          WHERE sapkto.pro_ver_id = :e_id\r\n        ENDEXEC.\r\n      endif.\r\n\
      \r\n  endif.\r\n\
      *\r\n\
      * Select für LU Konten\r\n  if i_kontonummer+0(2) = 'LU'.\r\n    move i_kontonummer+8(3) to l_n3.\r\n    move i_kontonummer+11(4) to t_nr.\r\n\
      *** MA,09.01.2009 ***\r\n\
      * Durch die Umstellung der UB-Nummern auf 2 Stellen,\r\n\
      * müssen hier auch die 2 Stellen übergeben werden.\r\n    move i_kontonummer+15(2) to u_n1.\r\n    EXEC SQL.\r\n      SELECT\r\n             ub.saldo_lfd,\r\n             ub.hauptbuch ,\r\n             to_char(ub.loeschdatum ,'YYYYMMDD') ,\r\n             to_char(ub.auslauf_dat ,'YYYYMMDD'),\r\n             ub.id,\r\n             ub.rueckst_tilg\r\n\
      *            konto_nr                                   \"KL\r\n      INTO  :e_saldo,\r\n            :e_hauptbuch ,\r\n            :e_loedatum ,\r\n            :e_ausldatum ,\r\n            :e_id ,\r\n            :e_tilrue\r\n        FROM produktverwendungen@ikbora pvw\r\n       ,pvw_beziehungen@ikbora pvwbe\r\n       ,pvw_beziehungstypen@ikbora pvwbety\r\n       ,produktverwendungen@ikbora ub\r\n\
      *       ,sap_konten@ikbora sapkto                        \"KL\r\n       WHERE pvw.id = pvwbe.pro_ver2_id\r\n         AND   PVW.BUCHUNGSKREIS = :w_bukrs\r\n         AND pvwbe.v_pvwty_id = pvwbety.id\r\n         AND ub.id = pvwbe.pro_ver1_id\r\n\
      *         AND UPPER(PVWBETY.KUERZEL) = 'KRUB'\r\n         AND pvwbety.kuerzel = 'KRUB'\r\n         AND pvw.kd_nr = :p_nr\r\n         AND pvw.lfd_nr = :l_n3\r\n         AND pvw.tranche = :t_nr\r\n         AND ub.lfd_nr = :u_n1\r\n         ORDER BY ub.loeschdatum DESC\r\n\
      *        AND ub.id=sapkto.pro_ver_id                     \"KL\r\n    ENDEXEC.\r\n\
      \r\n      if i_orkund eq 'X'.\r\n        EXEC SQL.\r\n          SELECT\r\n                 konto_nr\r\n          INTO\r\n                :e_kunnr\r\n          FROM\r\n               sap_konten@ikbora sapkto\r\n          WHERE sapkto.pro_ver_id = :e_id\r\n        ENDEXEC.\r\n      endif.\r\n\
      \r\n  endif.\r\n\
      \r\n  if sy-subrc ne 0.\r\n    raise konto_nicht_gefunden.\r\n  endif.\r\n\
      \r\n\
      endfunction. "
