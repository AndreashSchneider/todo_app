    status: Erledigt
    zeit: 
    aend_dat: 2011-03-21 00:00:00
    erfasst: 2011-01-19 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Es soll ein Excel-Report erstellt werden, der die Offenen Posten aller Verträge unter der Grundsatzzusage anzeigt. \r\n\
      \r\n\
      * *pk_abw1_ops_zur_grz.pkb,pka* und *abw1s001*\r\n\
      * steht auf Abnahme (28.01.2011) ikb3,ikb2,eingecheckt"
    mantis_nummer: "47873"
    aufgabe_id: 
    id: "3925"
    information: |-
      SELECT pvw.kd_nr kd_nr,

             pvw.lfd_nr lfd_nr,

             pvw.tranche tranche,

             pvw.buchungskreis,       

             cf.offen_betrag offen_betrag,

             cf.faellig_betrag faellig_betrag,

             cf.faellig_dat      

         FROM produktverwendungen gz

        JOIN pvw_zusatzdaten pvwzdat ON (pvwzdat.pro_ver_id = gz.id)

        JOIN pvw_beziehungen pvwbez ON (gz.id = pvwbez.pro_ver2_id)

        JOIN produktverwendungen pvw ON (pvwbez.pro_ver1_id = pvw.id)

        JOIN cf_zuordnungen cfzu ON (cfzu.pro_ver_id = pvw.id)

        JOIN cashflows cf ON (cf.cftzuo_id = cfzu.id)

      WHERE  pvwzdat.zusage_art = 'G'

         AND gz.auslauf_dat IS NULL

         AND gz.loeschdatum IS NULL

         AND gz.verwendungs_status = 'Z'

         AND cf.la_beleg_nr IS NOT NULL

         AND cf.v_verzg_schl IS NULL   

         AND EXISTS (SELECT ID

                           FROM vw_ver9_cf0_buschl_gruppen v

                          WHERE cf.ID = v.ID

                            AND faellig_az = 0)

         AND cf.offen_betrag > 0  

         AND cf.v_buschl_schl NOT IN (1, 6, 998)
