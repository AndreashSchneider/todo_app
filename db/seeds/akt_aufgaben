- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 2010-07-17 00:00:00
    erfasst: 2010-06-11 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Sabine Volk :auf der sf-Controlliste werden 4. Deals zwar auf dem Tab Arranger angezeigt auf dem Tab T-Masterliste hingegen nicht\r\n\
      \r\n\
      * 355325-1, 187972-1,182631-1,168067-1,407486-1\r\n\
      * Das liegt daran, dass die Masterliste noch die Tabelle *d_org_einheiten an die f_pfr_deals joined*\r\n\
      ** *AND AL2.ORG_ID_TEAM = AL11.ORG_ID(+)*  Mit Outer-Join würde der Satz angezeigt.\r\n\
      * Ansgar hat es angepasst und die Deals erscheinen seit heute wieder auf der Liste. Sabine per Mail informiert.\r\n\
      test_unterstrich"
    mantis_nummer: 
    aufgabe_id: 
    id: "3634"
    information: "-- es werden verschiedene Deals nicht auf der general-Seite der Excelliste angezeigt.\r\n\
      -- Das liegt daran, dass die Masterliste noch die Tabelle d_org_einheiten an die f_pfr_deals joint\r\n\
      -- ein OUTER-JOIN führt dazu, dass der Kunde auch in der Masterliste angezeigt wird!!\r\n\
      \r\n\
      SELECT *\r\n  FROM   d_pfr_deals dd\r\n       JOIN\r\n         f_pfr_deals fd\r\n       ON (fd.deal_id = dd.deal_id)\r\n WHERE kd_nr = 355325 AND lfd_nr = 1 AND fd.datum = TRUNC(SYSDATE - 1);\r\n\
      \r\n\
      ;\r\n\
      -- der Deal hat keinen Deal-Kaub\r\n  SELECT dpers.KBEZ\r\n       , dpers.KD_NR arranger\r\n       , dd.kd_nr\r\n       , dd.LFD_NR\r\n       , dd.DEAL_NAME\r\n\
      -- auf der Arranger-Reiterkarte wird der Deal angezeigt    \r\n\
      -- ausgehend von d_pfr_deals          \r\n    FROM DWHBI.D_ZEITEN AL1\r\n       , DWHBI.D_PFR_DEALS dd\r\n       , DWHBI.D_PERSONEN dpers\r\n       , DWHBI.F_PFR_PERS_ROLLEN f_per_rollen\r\n       , DWHBI.D_ROLLEN AL5\r\n       , DWHBI.F_PFR_DEALS AL6\r\n       , DWHBI.D_VERWEND_ARTEN AL8\r\n       , DWHBI.D_PERSONEN AL9\r\n   WHERE (    f_per_rollen.PERSON_ID = dpers.ID\r\n          AND f_per_rollen.DATUM = AL1.DATUM\r\n          AND f_per_rollen.ROLLE_ID = AL5.ROLLE_ID\r\n          AND AL6.DATUM = f_per_rollen.DATUM\r\n          AND AL6.DEAL_ID = f_per_rollen.DEAL_ID\r\n          AND AL6.PVA_ID = AL8.PVA_ID\r\n          AND dd.DEAL_ID = f_per_rollen.DEAL_ID\r\n          AND AL6.PERS_ID = AL9.ID)\r\n          AND dd.KD_NR =355325\r\n         AND((f_per_rollen.DATUM = to_date('15.05.2010','dd.mm.yyyy')\r\n              AND AL8.PVA_KUERZEL IN\r\n                     ('ACCO'\r\n                    , 'AERO'\r\n                    , 'AKBO'\r\n                    , 'AKDS'\r\n                    , 'AKIJ'\r\n                    , 'AKIS'\r\n                    , 'AKJU'\r\n                    , 'AKQF'\r\n                    , 'ALTE'\r\n                    , 'AUBP'\r\n                    , 'AUEK'\r\n                    , 'AUMZ'\r\n                    , 'AUPT'\r\n                    , 'AUSL'\r\n                    , 'AVAL'\r\n                    , 'All'\r\n                    , 'BAIV'\r\n                    , 'BAUL'\r\n                    , 'BPBP'\r\n                    , 'BPEK'\r\n                    , 'BPMZ'\r\n                    , 'BRID'\r\n                    , 'BUER'\r\n                    , 'CARE'\r\n                    , 'CLOI'\r\n                    , 'ENER'\r\n                    , 'EXBB'\r\n                    , 'EXBS'\r\n                    , 'EXDS'\r\n                    , 'EXPR'\r\n                    , 'EXSB'\r\n                    , 'FIKB'\r\n                    , 'FIRE'\r\n                    , 'FRMO'\r\n                    , 'FXEI'\r\n                    , 'FXFE'\r\n                    , 'FXMW'\r\n                    , 'FXSB'\r\n                    , 'FXSI'\r\n                    , 'FXWR'\r\n                    , 'FXZA'\r\n                    , 'GEMI'\r\n                    , 'GEWE'\r\n                    , 'HAND'\r\n                    , 'HOTE'\r\n                    , 'ICAP'\r\n                    , 'ICMS'\r\n                    , 'ICOT'\r\n                    , 'ICPG'\r\n                    , 'ICPS'\r\n                    , 'ICRR'\r\n                    , 'ICSP'\r\n                    , 'ICTC'\r\n                    , 'ICTR'\r\n                    , 'ICUN'\r\n                    , 'IFBO'\r\n                    , 'IGKZ'\r\n                    , 'INAK'\r\n                    , 'INBB'\r\n                    , 'INBO'\r\n                    , 'INCO'\r\n                    , 'INDE'\r\n                    , 'INDS'\r\n                    , 'INJU'\r\n                    , 'INMK'\r\n                    , 'ISON'\r\n                    , 'JINU'\r\n                    , 'KFZI'\r\n                    , 'KOED'\r\n                    , 'KOZZ'\r\n                    , 'KWBK'\r\n                    , 'KWBQ'\r\n                    , 'KWED'\r\n                    , 'KWFE'\r\n                    , 'KWFZ'\r\n                    , 'KWGA'\r\n                    , 'KWGB'\r\n                    , 'KWGE'\r\n                    , 'KWGG'\r\n                    , 'KWIW'\r\n                    , 'KWLB'\r\n                    , 'KWME'\r\n                    , 'KWML'\r\n                    , 'KWUE'\r\n                    , 'LDDL'\r\n                    , 'LDFF'\r\n                    , 'LDTN'\r\n                    , 'LEMF'\r\n                    , 'LESF'\r\n                    , 'LETN'\r\n                    , 'LIFT'\r\n                    , 'LIQL'\r\n                    , 'LOGI'\r\n                    , 'LSOS'\r\n                    , 'MASC'\r\n                    , 'MKAD'\r\n                    , 'MKAL'\r\n                    , 'MKAR'\r\n                    , 'MKBA'\r\n                    , 'MKBI'\r\n                    , 'MKBO'\r\n                    , 'MKDS'\r\n                    , 'MKEI'\r\n                    , 'MKFE'\r\n                    , 'MKFZ'\r\n                    , 'MKJU'\r\n                    , 'MKMA'\r\n                    , 'MKMI'\r\n                    , 'MKMW'\r\n                    , 'MKSD'\r\n                    , 'MKWR'\r\n                    , 'MKZA'\r\n                    , 'PBAU'\r\n                    , 'PEAU'\r\n                    , 'PEBP'\r\n                    , 'PEEK'\r\n                    , 'PEIN'\r\n                    , 'PEMZ'\r\n                    , 'PENS'\r\n                    , 'PEPT'\r\n                    , 'PFAU'\r\n                    , 'PFBF'\r\n                    , 'PFBM'\r\n                    , 'PFBO'\r\n                    , 'PFDE'\r\n                    , 'PFDS'\r\n                    , 'PFEN'\r\n                    , 'PFIP'\r\n                    , 'PFJU'\r\n                    , 'PFPG'\r\n                    , 'PFSI'\r\n                    , 'PFSM'\r\n                    , 'PFSO'\r\n                    , 'PFSP'\r\n                    , 'PFSW'\r\n                    , 'PFTE'\r\n                    , 'PFUN'\r\n                    , 'PFWI'\r\n                    , 'PFWM'\r\n                    , 'PMAU'\r\n                    , 'PMDE'\r\n                    , 'PMIE'\r\n                    , 'PMOD'\r\n                    , 'PORE'\r\n                    , 'PORT'\r\n                    , 'POSO'\r\n                    , 'POWI'\r\n                    , 'PPKW'\r\n                    , 'PPPA'\r\n                    , 'PPPD'\r\n                    , 'PPPS'\r\n                    , 'PRIS'\r\n                    , 'PRPL'\r\n                    , 'PSON'\r\n                    , 'PTAU'\r\n                    , 'PTBP'\r\n                    , 'PTEK'\r\n                    , 'PTMZ'\r\n                    , 'PUMZ'\r\n                    , 'RAIL'\r\n                    , 'REHA'\r\n                    , 'REST'\r\n                    , 'ROAD'\r\n                    , 'ROLL'\r\n                    , 'SCHO'\r\n                    , 'SFIF'\r\n                    , 'SHBK'\r\n                    , 'SHCO'\r\n                    , 'SHEX'\r\n                    , 'SHGA'\r\n                    , 'SHKR'\r\n                    , 'SHMA'\r\n                    , 'SHOL'\r\n                    , 'SHPT'\r\n                    , 'SHRO'\r\n                    , 'SHSO'\r\n                    , 'SIAU'\r\n                    , 'SIDE'\r\n                    , 'SOIM'\r\n                    , 'SONS'\r\n                    , 'SPFR'\r\n                    , 'STT'\r\n                    , 'STTC'\r\n                    , 'STZ'\r\n                    , 'STZC'\r\n                    , 'STZT'\r\n                    , 'TOLL'\r\n                    , 'TUNN'\r\n                    , 'WAST'\r\n                    , 'WOHN'\r\n                    , 'WTER'\r\n                    , 'n/a')\r\n              AND 1 = 1));\r\n\
      --ORDER BY 1, 5;\r\n\
      \r\n\
      \r\n\
      --- Und jetzt die Masterliste\r\n\
      --  Mit outer-Join auf d_org_einheiten funktioniert die Abfrage\r\n\
      SELECT AL2.DATUM\r\n     , AL2.DEAL_ID\r\n     , d_per.KD_NR\r\n     , AL10.LFD_NR\r\n     , d_per.KBEZ\r\n     , AL10.DEAL_NAME\r\n     , AL2.ORG_ID_PCS\r\n     , AL2.ORG_ID_SCS\r\n     , AL10.DEAL_KAUB\r\n     , AL4.PEBE_MITA_KBEZ_JURB\r\n     , AL6.WIRT_BONI_IKB\r\n     , AL9.MOODYS_MOODYS_ID\r\n     , AL7.PERFORMANCE_SCORE\r\n     , AL2.LATEST_MANAGEMENT_ACCOUNT\r\n     , AL10.ERFUELLUNG_KZ\r\n     , AL2.DEAL_AMOUNT\r\n     , AL2.IKB_RISK\r\n     , AL2.DRAWN_OBLIGO\r\n     , AL2.TOT_DEBT_EBITDA\r\n     , AL2.SEN_DEBT_EBITDA\r\n     , AL10.IKB_ROLLE\r\n     , AL10.STANDORT\r\n     , AL11.TEAM_LTEXT\r\n     , DECODE(AL5.GRP_KUERZEL, 'AK', 'AK', 'PF')\r\n     , AL5.GRP_NAME\r\n     , AL5.PVA_KUERZEL\r\n     , AL5.NAME\r\n     , d_lae.LAND_NAME\r\n     , AL11.TEAM_NR\r\n     , d_lae.LAND_GRP_NAME\r\n     , AL9.MOODYS_MOODY_SCHL\r\n     , AL9.MOODYS_MOODY_NAME\r\n    -- , AL14.WZBRA_ANZEIGE\r\n     , AL15.bacchus_warehoused\r\n     , AL15.bacchus_actual\r\n     , AL10.CLO\r\n     , AL16.amount_seas\r\n     , AL10.DEAL_SI_RECOVERY_CLASS\r\n     , AL2.SI_DEAL_BONI\r\n     , AL2.RISIKOFAKTOR\r\n     , AL12.RISA_STATUS_KUERZEL\r\n     , AL10.BID\r\n     , AL10.ISSUER_NAME\r\n     , AL10.RESTRUKTURIERT_KZ\r\n     , AL10.RECAP_DEAL_KZ\r\n     , AL10.DEAL_BEWILL_DAT\r\n     --, AL13.EWB / 1000\r\n     , AL2.BEWILL_BETRAG_EUR\r\n     , AL2.DLM_GESCH_FINAL_WAE / 1000\r\n     , AL2.DLM_GESCH_VOL_FINAL / 1000\r\n     , AL2.DLM_GESCH_VOL_PROT / 1000\r\n     , AL2.DLM_OBLIGO_FINAL / 1000\r\n     , AL2.DLM_OBLIGO_PROT / 1000\r\n     , AL2.UW_ERHALTEN_GV / 1000\r\n     , AL2.UW_ERHALTEN_OBLIGO / 1000\r\n     , AL2.UW_GENEHMIGT_GV / 1000\r\n     , AL2.UW_GENEHMIGT_OBLIGO / 1000\r\n     , AL10.LE_TIL_DAT\r\n     , AL2.TOTAL_OBLIGO / 1000\r\n     , AL10.EINSTUFUNGS_KZ\r\n    -- , AL13.EWB / 1000\r\n     , AL10.WIRT_BONI_EV\r\n     , AL10.SICH_BONI_EV\r\n     , AL10.WAEHR_CODE_DEAL\r\n     , AL10.WAEHR_CODE_BILANZ\r\n    -- , AL14.WZBRA_NAME\r\n     , AL10.STANDORT\r\n  FROM DWHBI.D_PFR_LAENDER d_lae\r\n     , DWHBI.F_PFR_DEALS AL2\r\n     , DWHBI.D_PERSON_BEARBEITER AL4\r\n     , DWHBI.D_VERWEND_ARTEN AL5\r\n     , DWHBI.D_BONITAETEN AL6\r\n     , DWHBI.D_PFR_PERFORMANCE_SCORES AL7\r\n     , DWHBI.D_PERSONEN d_per\r\n     , DWHBI.D_MOODYS_SEKTOREN AL9\r\n     , DWHBI.D_PFR_DEALS AL10\r\n     , DWHBI.D_ORG_EINHEITEN AL11\r\n     , DWHBI.D_RISA_STATUS AL12\r\n    -- , DWHBI.F_PFR_EWB_DEALS AL13\r\n     , DWHBI.D_BRANCHEN AL14\r\n     , (  SELECT D14AL1.DEAL_ID AS deal_id\r\n               , D14AL1.DATUM AS datum\r\n               , SUM(D14AL1.MARK_VOLUMEN) AS bacchus_warehoused\r\n               , SUM(D14AL1.AKT_VOLUMEN) AS bacchus_actual\r\n            FROM DWHBI.F_PFR_CLO_DATEN D14AL1, DWHBI.D_PLA_PLATZIERUNGEN D14AL3\r\n           WHERE (D14AL1.PLA_ID = D14AL3.PLA_ID)\r\n                 AND(D14AL3.KUERZEL IN ('BA01', 'BA02'))\r\n        GROUP BY D14AL1.DEAL_ID, D14AL1.DATUM) AL15\r\n     , (  SELECT D15AL1.DEAL_ID AS deal_id\r\n               , D15AL1.DATUM AS datum\r\n               , SUM(D15AL1.CLO_VOLUMEN) AS amount_seas\r\n            FROM DWHBI.F_PFR_CLO_DATEN D15AL1, DWHBI.D_PLA_PLATZIERUNGEN D15AL3\r\n           WHERE (D15AL1.PLA_ID = D15AL3.PLA_ID) AND (D15AL3.KUERZEL LIKE 'SF%')\r\n        GROUP BY D15AL1.DEAL_ID, D15AL1.DATUM) AL16\r\n WHERE (    AL2.PEBE_ID = AL4.PEBE_ID(+)\r\n   --     AND AL2.DATUM = AL13.DATUM(+)\r\n   --     AND AL2.DEAL_ID = AL13.DEAL_ID(+)\r\n        AND AL14.ID(+) = AL2.D_BRA_ID\r\n        AND AL2.DEAL_ID = AL15.deal_id(+)\r\n        AND AL2.DATUM = AL15.datum(+)\r\n        AND AL2.DATUM = AL16.datum(+)\r\n        AND AL2.DEAL_ID = AL16.deal_id(+)\r\n        AND AL6.ID = AL2.BONI_ID\r\n        AND d_per.ID = AL2.PERS_ID\r\n        AND AL10.DEAL_ID = AL2.DEAL_ID\r\n        AND AL2.LAND_ID = d_lae.LAND_ID\r\n        AND AL9.MOODYS_MOODYS_ID = AL2.MOODYS_ID\r\n        AND AL2.PVA_ID = AL5.PVA_ID\r\n        AND AL7.PSCORE_ID = AL2.PSCORE_ID\r\n        AND AL2.RISA_ID = AL12.RISA_ID\r\n        AND AL2.ORG_ID_TEAM = AL11.ORG_ID(+)\r\n   )\r\n        AND d_per.KD_NR=355325\r\n       AND((AL2.DATUM = to_date('15.05.2010','dd.mm.yyyy')\r\n            AND AL5.PVA_KUERZEL IN\r\n                   ('ACCO'\r\n                  , 'AERO'\r\n                  , 'AKBO'\r\n                  , 'AKDS'\r\n                  , 'AKIJ'\r\n                  , 'AKIS'\r\n                  , 'AKJU'\r\n                  , 'AKQF'\r\n                  , 'ALTE'\r\n                  , 'AUBP'\r\n                  , 'AUEK'\r\n                  , 'AUMZ'\r\n                  , 'AUPT'\r\n                  , 'AUSL'\r\n                  , 'AVAL'\r\n                  , 'All'\r\n                  , 'BAIV'\r\n                  , 'BAUL'\r\n                  , 'BPBP'\r\n                  , 'BPEK'\r\n                  , 'BPMZ'\r\n                  , 'BRID'\r\n                  , 'BUER'\r\n                  , 'CARE'\r\n                  , 'CLOI'\r\n                  , 'ENER'\r\n                  , 'EXBB'\r\n                  , 'EXBS'\r\n                  , 'EXDS'\r\n                  , 'EXPR'\r\n                  , 'EXSB'\r\n                  , 'FIKB'\r\n                  , 'FIRE'\r\n                  , 'FRMO'\r\n                  , 'FXEI'\r\n                  , 'FXFE'\r\n                  , 'FXMW'\r\n                  , 'FXSB'\r\n                  , 'FXSI'\r\n                  , 'FXWR'\r\n                  , 'FXZA'\r\n                  , 'GEMI'\r\n                  , 'GEWE'\r\n                  , 'HAND'\r\n                  , 'HOTE'\r\n                  , 'ICAP'\r\n                  , 'ICMS'\r\n                  , 'ICOT'\r\n                  , 'ICPG'\r\n                  , 'ICPS'\r\n                  , 'ICRR'\r\n                  , 'ICSP'\r\n                  , 'ICTC'\r\n                  , 'ICTR'\r\n                  , 'ICUN'\r\n                  , 'IFBO'\r\n                  , 'IGKZ'\r\n                  , 'INAK'\r\n                  , 'INBB'\r\n                  , 'INBO'\r\n                  , 'INCO'\r\n   