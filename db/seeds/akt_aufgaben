- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2010-03-18 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Tac 5s308 ist dem Marktprofil aber nicht Ra-Profil mit Freigabe zugeordnet, obwohl sie  \"UF-Bestand Votum Marktfolge\" heißt\r\n\
      \r\n\
      * Anfrage von Denise Abels\r\n\
      * die F-Funktion dieser Tac wird nicht zur Freigabe verwendet, da wir hierfür die Funktionalen Tacs haben."
    mantis_nummer: 
    aufgabe_id: "1157"
    id: "3464"
    information: |
      /*

      bik5_markt            = Z

      bik5_markt_komp       = Z,N,K,L

      bik5_produkt_vertrieb = Z..F

      bik5_marktfolge       = Z,N,K,L

      */

      --------------------------------------------------------------------------------

      -- Frage: In welchem Profil ist die Tac xy enthalten?

      --------------------------------------------------------------------------------

      SELECT tac.tac_name,prof.profil_name,proftac.funktion_z z,

             proftac.funktion_n n,proftac.funktion_k k,

             proftac.funktion_l l,proftac.funktion_f f,proftac.funktion_s s,tac.kbez

        FROM vw_glb1_transaktionen_dbs tac

        JOIN profile_tacs proftac ON (proftac.v_tacdb_id= tac.id)

        JOIN PROFILE prof ON (prof.id = proftac.v_prof_id)

       WHERE tac.tac_name = '5S308';


    projekt: Ent.Vorlage
    heute_bearbeiten_kz: N
