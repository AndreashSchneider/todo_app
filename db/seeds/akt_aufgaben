      -- Skript für die 70er Verträge\r\n\
      DECLARE \r\n\
      CURSOR crpvw IS \r\n\
      SELECT * FROM produktverwendungen \r\n  where buchungskreis = 70\r\n  AND verwendungs_status = 'I'\r\n\
