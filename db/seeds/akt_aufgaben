      SELECT scs.v_oeinh_id, pvw.kd_nr, pvw.lfd_nr,pvw.typ, scs.*

        FROM orgscs_pvw_zuordnungen scs JOIN service_arten sa ON(sa.ID = scs.v_serv_id)

             JOIN produktverwendungen pvw ON(scs.pro_ver_id = pvw.ID)

       WHERE sa.kuerzel = 'VERM' AND pvw.typ = 'DEAL';

       SELECT * FROM all_objects WHERE lower(object_name) LIKE '%avs_umschl%'

      

      PK_PER0_AVS_UMSCHL
