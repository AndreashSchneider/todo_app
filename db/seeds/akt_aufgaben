- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2009-04-30 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "1.Funktion die einen Performance_Score_ist_5= J/N  liefert -- > Was ist, wenn der Kunde keinen Performance-Score hat, anderer Kunde im Deal führend? \r\n\
      Keinen Deal ? \r\n\
      Deal in Bearbeitung?\r\n\
      Aussage von Frau Werr\r\n\
      Kein Performance_Score bedeutet Performance_Score ist nicht 5!!"
    mantis_nummer: "31677"
    aufgabe_id: 
    id: "2449"
    information: |-
      DECLARE

        CURSOR crDeals 

            IS 

        SELECT * 

          FROM vw_dlm0_deal_kunden;

      

       FUNCTION fk_Performance_Score_ist_5 (inKdNr produktverwendungen.kd_nr%TYPE) 

         RETURN VARCHAR2

         IS  

         CURSOR crKdDeal (inKdNr produktverwendungen.kd_nr%TYPE ) 

             IS

         SELECT DISTINCT pro_ver_id 

           FROM vw_dlm0_deal_kunden 

          WHERE kd_nr_vertrag = inKdNr;

          nPerformance NUMBER;

          io_t_einzelkrit pk_dlm0_performance_score.type_tab_einzelkrit;

        BEGIN

          FOR rcKdDeal IN crKdDeal(inKdNr) LOOP 

            IF pk_dlm0_performance_score.fk_berechne_ps(rcKdDeal.pro_ver_id,trunc(sysdate),io_t_einzelkrit) = 5 

            THEN

               RETURN 'J';

            END IF;

          END LOOP;

          RETURN 'N';

        END;

      

      BEGIN 

       FOR rcDeals IN crDeals LOOP

         dbms_output.put_line('Kunde hat Performance-Score von 5? '||fk_Performance_Score_ist_5(rcDeals.kd_nr_vertrag));

       END LOOP;

        

      END;
