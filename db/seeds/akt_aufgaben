    information: |-
      (

      SELECT pro_ver.id,

             pro_ver.kd_nr,

      --       pk_dlm0_basis.fk_ermittle_verwendungsart(pro_ver.id)  pvw,

             v_oteam.kbez team,

      --       pk_glb2_bearb_schl.fk_marb_pers_nr(pro_ver.id, 'VERT', 'JURB', 'TRUE') jurb_kd_nr,

      --       pk_ver2_impairment.fk_restrukt_vertrag(pro_ver.id) restructured_kz,

             pvw_bez.pro_ver2_id  pro_ver_id_deal,

             0  pro_ver_id_gsz,

             pro_ver.typ,

             NULL

        FROM produktverwendungen pro_ver

        JOIN pvw_beziehungen              pvw_bez   ON (pvw_bez.pro_ver1_id = pro_ver.id)

        JOIN vw_glb1_pvw_beziehungstypen  vwvpvwty  ON (vwvpvwty.id = pvw_bez.v_pvwty_id)

        LEFT OUTER JOIN org_einheiten     v_oeinh   ON (v_oeinh.id = pro_ver.v_oeinh_pcs_id)

        LEFT OUTER JOIN org_regionen      v_oreg    ON (v_oreg.id = v_oeinh.v_oreg_id)

        LEFT OUTER JOIN org_teams         v_oteam   ON (v_oteam.id = v_oreg.v_oteam_id)

       WHERE vwvpvwty.kuerzel = 'DEAL'

         AND pro_ver.auslauf_dat IS NULL

         AND pro_ver.loeschdatum IS null

      UNION ALL

      SELECT pro_ver.id,

             pro_ver.kd_nr,

      --       pk_dlm0_basis.fk_ermittle_verwendungsart(pro_ver.id)  pvw,

             v_oteam.kbez team,

      --       pk_glb2_bearb_schl.fk_marb_pers_nr(pro_ver.id, 'VERT', 'JURB', 'TRUE') jurb_kd_nr,

      --       pk_ver2_impairment.fk_restrukt_vertrag(pro_ver.id) restructured_kz,

             pvw_bez.pro_ver2_id  pro_ver_id_deal,

             pvw_bez.pro_ver1_id  pro_ver_id_gsz,

             pro_ver.typ,

             vwvpvwty2.kuerzel

        FROM produktverwendungen pro_ver

        JOIN pvw_beziehungen              pvw_bez2  ON (pvw_bez2.pro_ver1_id = pro_ver.id)

        JOIN pvw_beziehungen              pvw_bez   ON (pvw_bez.pro_ver1_id = pvw_bez2.pro_ver2_id)

        JOIN vw_glb1_pvw_beziehungstypen  vwvpvwty  ON (vwvpvwty.id = pvw_bez.v_pvwty_id)

        JOIN vw_glb1_pvw_beziehungstypen  vwvpvwty2 ON (vwvpvwty2.id = pvw_bez2.v_pvwty_id)

        JOIN pvw_zusatzdaten              pvwzdat   ON (pvwzdat.pro_ver_id = pvw_bez.pro_ver1_id)

        LEFT OUTER JOIN org_einheiten     v_oeinh   ON (v_oeinh.id = pro_ver.v_oeinh_pcs_id)

        LEFT OUTER JOIN org_regionen      v_oreg    ON (v_oreg.id = v_oeinh.v_oreg_id)

        LEFT OUTER JOIN org_teams         v_oteam   ON (v_oteam.id = v_oreg.v_oteam_id)

       WHERE vwvpvwty.kuerzel = 'DEAL'

         AND pvwzdat.zusage_art = 'G'

         AND pro_ver.auslauf_dat IS NULL

         AND pro_ver.loeschdatum IS null

      )

      MINUS

      (

      SELECT pro_ver.id,

             pro_ver.kd_nr,

      --       pk_dlm0_basis.fk_ermittle_verwendungsart(pro_ver.id)  pvw,

             v_oteam.kbez team,

      --       pk_glb2_bearb_schl.fk_marb_pers_nr(pro_ver.id, 'VERT', 'JURB', 'TRUE') jurb_kd_nr,

      --       pk_ver2_impairment.fk_restrukt_vertrag(pro_ver.id) restructured_kz,

             pvw_bez.pro_ver2_id  pro_ver_id_deal,

             0  pro_ver_id_gsz,

             pro_ver.typ,

             NULL

        FROM produktverwendungen pro_ver

        JOIN pvw_beziehungen              pvw_bez   ON (pvw_bez.pro_ver1_id = pro_ver.id)

        JOIN vw_glb1_pvw_beziehungstypen  vwvpvwty  ON (vwvpvwty.id = pvw_bez.v_pvwty_id)

        LEFT OUTER JOIN org_einheiten     v_oeinh   ON (v_oeinh.id = pro_ver.v_oeinh_pcs_id)

        LEFT OUTER JOIN org_regionen      v_oreg    ON (v_oreg.id = v_oeinh.v_oreg_id)

        LEFT OUTER JOIN org_teams         v_oteam   ON (v_oteam.id = v_oreg.v_oteam_id)

       WHERE vwvpvwty.kuerzel = 'DEAL'

         AND pro_ver.auslauf_dat IS NULL

         AND pro_ver.loeschdatum IS null

      UNION ALL

      SELECT pro_ver.id,

             pro_ver.kd_nr,

      --       pk_dlm0_basis.fk_ermittle_verwendungsart(pro_ver.id)  pvw,

             v_oteam.kbez team,

      --       pk_glb2_bearb_schl.fk_marb_pers_nr(pro_ver.id, 'VERT', 'JURB', 'TRUE') jurb_kd_nr,

      --       pk_ver2_impairment.fk_restrukt_vertrag(pro_ver.id) restructured_kz,

             pvw_bez.pro_ver2_id  pro_ver_id_deal,

             pvw_bez.pro_ver1_id  pro_ver_id_gsz,

             pro_ver.typ,

             vwvpvwty2.kuerzel

        FROM produktverwendungen pro_ver

        JOIN pvw_beziehungen              pvw_bez2  ON (pvw_bez2.pro_ver1_id = pro_ver.id)

        JOIN pvw_beziehungen              pvw_bez   ON (pvw_bez.pro_ver1_id = pvw_bez2.pro_ver2_id)

        JOIN vw_glb1_pvw_beziehungstypen  vwvpvwty  ON (vwvpvwty.id = pvw_bez.v_pvwty_id)

        JOIN vw_glb1_pvw_beziehungstypen  vwvpvwty2 ON (vwvpvwty2.id = pvw_bez2.v_pvwty_id)

        JOIN pvw_zusatzdaten              pvwzdat   ON (pvwzdat.pro_ver_id = pvw_bez.pro_ver1_id)

        LEFT OUTER JOIN org_einheiten     v_oeinh   ON (v_oeinh.id = pro_ver.v_oeinh_pcs_id)

        LEFT OUTER JOIN org_regionen      v_oreg    ON (v_oreg.id = v_oeinh.v_oreg_id)

        LEFT OUTER JOIN org_teams         v_oteam   ON (v_oteam.id = v_oreg.v_oteam_id)

       WHERE vwvpvwty.kuerzel = 'DEAL'

         AND ((pro_ver.typ = 'IR' AND vwvpvwty2.kuerzel = 'GZIR') OR pro_ver.typ != 'IR')

         AND pvwzdat.zusage_art = 'G'

         AND pro_ver.auslauf_dat IS NULL

         AND pro_ver.loeschdatum IS null

      )
