      i_t_dlm_jahrw ikbobj.pk_dlm0_financials_trans.t_dlm_jahrw;

      BEGIN

        i_t_dlm_jahrw := i_t_dlm_jahrw;

        ikbobj.pk_dlm0_financials_trans.pr_dlmjahrw_query(

          i_t_dlm_jahrw=>i_t_dlm_jahrw,

          i_n_pro_ver_id=>1602406,

          i_d_gj_beginn=>to_date('01.09.2009','dd.mm.yyyy'),

          i_vc_bereich=>'P');

       FOR i IN i_t_dlm_jahrw.FIRST .. i_t_dlm_jahrw.LAST LOOP

          dbms_output.put_line('id                '||i_t_dlm_jahrw(i).id                );

          dbms_output.put_line('pro_ver_id        '||i_t_dlm_jahrw(i).pro_ver_id        );

