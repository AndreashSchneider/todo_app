      JOIN ze_zuordnungen zezu ON (ze.id = zezu.zlgeing_id)\r\n\
      JOIN cashflows cf ON (cf.id = zezu.cashfl_id)\r\n\
      WHERE ze.kd_nr = 172288\r\n\
