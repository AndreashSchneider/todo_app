      require 'rubygems'

      require 'rake'

      # Ausgangsverzeichnis

      Dir.chdir('X:/DS/sql/ikb/ikbobj/')

      # Zielverzeichnis

      vcDirZiel ='X:/DS/sql/ikb/ikbobj/restr_referenzen_weg'

      # Suchausdruck bestimmen

      suche = Regexp.new(/pragma\s*?restrict_references.*?\);/i);

        liste_restr_ref =[]

        FileList["**/*.*"].egrep(suche) do |a,b,c |

        	  p "#{a} und #{b} mit #{c}"

             File.open(vcDirZiel+"/#{a}",'w') do |file|

             	 file2 = File.read(a).gsub(suche,'')

             	 file.write(file2)

             	 liste_restr_ref << a

             end unless liste_restr_ref.include? a     

        end
