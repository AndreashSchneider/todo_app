- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2009-04-30 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Funktion die SeniorDebt/EBITDA > 5,5 = J/N liefert. Wir liefern hier den ISTwert SeniorDebt/EBITDA\r\n\
      Frau Werr fragen. Kein SeniorDebt/EBITDA = N !!!\r\n\
      1. Was ist mit Kunden ohne Deal \r\n\
      2. mit Deal in Bearbeitung\r\n\
      3. mit Vertrag unter anderem führenden Kunden."
    mantis_nummer: "31676"
    aufgabe_id: 
    id: "2450"
    information: |-
      DECLARE

      CURSOR crDeals IS 

      SELECT * FROM vw_dlm0_deal_kunden;

      FUNCTION fk_sen_debt_ebitda_gt_5_5(inKdNr personen.kd_nr%TYPE)  

       RETURN VARCHAR IS 

        CURSOR crSenDeb_Ebitda (inProVerId produktverwendungen.id%TYPE ) IS

          SELECT sen_debt_ebitda_ist 

          FROM dlm_kennzahlen 

         WHERE pro_ver_id = inProVerId 

           AND tot_debt_ebitda_ist IS NOT NULL

          ORDER BY datum desc;

           

        CURSOR crKdDeal (inKdNr produktverwendungen.kd_nr%TYPE ) IS

         SELECT DISTINCT pro_ver_id 

         FROM vw_dlm0_deal_kunden 

          WHERE kd_nr_vertrag = inKdNr;

               

        nSenDebt_ebitda NUMBER;

      BEGIN 

        FOR rcKdDeal IN crKdDeal(inKdNr) LOOP 

          OPEN crSenDeb_Ebitda(rcKdDeal.pro_ver_id);

          FETCH crSenDeb_Ebitda INTO nSenDebt_ebitda;

          IF crSenDeb_Ebitda%NOTFOUND THEN 

            nSenDebt_ebitda :=0;

          END IF;

          CLOSE crSenDeb_Ebitda;

          IF nSenDebt_ebitda>5.5 THEN 

            RETURN 'J';        

          END IF;

        END LOOP;

        RETURN 'N';

      END;

      BEGIN 

       FOR rcDeals IN crDeals LOOP

       dbms_output.put_line('Kunde :'||rcDeals.kd_nr_vertrag||' hat sendebt/ebitda > 5.5? ' ||fk_sen_debt_ebitda_gt_5_5(rcDeals.kd_nr_vertrag));

       END LOOP;

      END;
    projekt: Ent.Vorlage
