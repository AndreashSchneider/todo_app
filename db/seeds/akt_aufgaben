- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2010-09-06 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Watermark Rails\r\n\
      # watermark.rb für Rails nach lib/paperclip_processors/ kopieren\r\n\
      # paperclip installieren\r\n\
      # gemfile eintragen\r\n\
      # railtie einbinden \r\n\
      # einträge in die Forms\r\n\
      # Einträge in die Modelle zum Zugriff auf die Funktionen \r\n\
      # Monkeypatch für Windows in Environment.rb\r\n\
      # Wichtig ist der Pfad nach ImageMagick !!"
    mantis_nummer: 
    aufgabe_id: 
    id: "3732"
    information: " Paperclip installieren\r\n\
      gem install --local x:\\Downloads\\paperclip-2.3.3.gem  \r\n\
      \r\n\
      # Eintrag in die Datei gemfile \r\n\
      gem 'paperclip'\r\n\
      \r\n\
      # in die Datei application.rb\r\n\
      Paperclip::Railtie.insert   \r\n\
      \r\n\
      # generieren des db-Skript für neue Spalten\r\n\
      ->rails generate paperclip page photo   \r\n\
      \r\n\
      # anlegen der Spalten in der Datenbank\r\n\
      ->rake db:migrate\r\n\
      \r\n\
      # Serverstarten\r\n\
      ->rails server \r\n\
      \r\n\
      \r\n\
      # Eintrag in der datei model/page.rb, damit die sind die Zugriffe auf das object photo\r\n\
      # von page aus möglich z.B. page.photo.exists?\r\n\
      require 'paperclip_processors/watermark' # falls ein Wasserzeichen eingegeben werden soll\r\n\
      # die Datei watermark.rb muss dafür vorhanden sein!!\r\n\
      has_attached_file :photo, :processors => [:watermark],\r\n    :styles => {:thumb => '50x50#',\r\n                :preview => {\r\n                  :geometry => '640x480>',\r\n                  :watermark_path => \"#{Rails.root}/public/images/watermark.png\",\r\n                  :position => 'Center'\r\n                   },\r\n                 :small  => \"150x150>\",\r\n                 :medium => \"300x300>\"\r\n    }\r\n    \r\n\
      # Eintrag in der Datei views/edit.erb and new.erb, damit man eine Datei hochladen kann    \r\n\
      <% form_for(@page,:html => { :multipart => true }) do |f| %>\r\n  <%= f.error_messages %>\r\n  <%= render :partial => 'form', :locals => { :f => f } %>\r\n\
      <% end %>   \r\n \r\n\
      # Eintrag in die Datei views/_form.erb\r\n\
      <p>\r\n  <%= f.label 'Photo' %><br />\r\n  <%= f.file_field :photo %>\r\n\
      </p>\r\n\
      \r\n\
      # Anlegen der Datei \\config\\initializers\\paperclip.rb, damit der Pfad gefunden wird\r\n\
      Paperclip.options[:command_path]='x:\\\\Schneider\\\\Software\\\\ImageMagick-6.5.6-Q8\\\\'\r\n\
      Paperclip.options[:swallow_stderr] = false\r\n\
      \r\n\
      # rails console # zum Testen in der console, ob das Bild gefunden wird\r\n\
      p Paperclip::Geometry.from_file(RAILS_ROOT + '/public/images/watermark.png')\r\n\
      \r\n\
      # Eintrag in index.html.erb, damit wird das Foto auch in der Liste angezeigt.\r\n\
      <% if page.photo.exists? then %>\r\n  <td>Small:<%= image_tag page.photo.url(:thumb) %><td>\r\n\
      <% end %>\r\n\
      \r\n\
      # Eintrag in environment.rb\r\n\
      \r\n\
      # Monkeypatch für Windows-Bug !!!\r\n\
      # Special options for Win32\r\n\
      \tif RUBY_PLATFORM.include?(\"mingw32\") \r\n\
      \t  # monkey patch for class method in paperclip.rb\r\n\
      \t  # overwriting quote_command_options method\r\n\
      \t  module Paperclip\r\n\
      \t    class << self\r\n\
      \t      def quote_command_options(*options)\r\n\
      \t        options.map do |option|\r\n\
      \t          option.split(\"'\").map{|m| \"\\\"#{m}\\\"\"}.join(\"\\\\'\") \r\n \t        end\r\n \t      end\r\n \t    end\r\n \t  end\r\n  end\r\n\
      \r\n\
      # hier der Unterschied ! \r\n\
      'x:\\\\Schneider\\\\Software\\\\ImageMagick-6.5.6-Q8\\\\'.split(\"'\").map{|m| \"'#{m}'\"  }.join(\"\\\\'\")\r\n\
      'x:\\\\Schneider\\\\Software\\\\ImageMagick-6.5.6-Q8\\\\'.split(\"'\").map{|m| \"\\\"#{m}\\\"\"}.join(\"\\\\'\")\r\n\
      module Paperclip\r\n  class Watermark < Processor\r\n    # Handles watermarking of images that are uploaded.\r\n    attr_accessor :current_geometry, :target_geometry, :format, :whiny, :convert_options, :watermark_path, :overlay, :position\r\n    \r\n    def initialize file, options = {}, attachment = nil\r\n       super\r\n       geometry = options[:geometry]\r\n       @file = file\r\n       @crop = geometry[-1,1] == '#'\r\n       @target_geometry = Geometry.parse geometry\r\n       @current_geometry = Geometry.from_file @file\r\n       @convert_options = options[:convert_options]\r\n       @whiny = options[:whiny].nil? ? true : options[:whiny]\r\n       @format = options[:format]\r\n       @watermark_path = options[:watermark_path]\r\n       @position = options[:position].nil? ? \"SouthEast\" : options[:position]\r\n       @overlay = options[:overlay].nil? ? true : false\r\n       @current_format = File.extname(@file.path)\r\n       @basename = File.basename(@file.path, @current_format)\r\n     end\r\n     \r\n     # TODO: extend watermark\r\n     \r\n     # Returns true if the +target_geometry+ is meant to crop.\r\n      def crop?\r\n        @crop\r\n      end\r\n\
      \r\n      # Returns true if the image is meant to make use of additional convert options.\r\n      def convert_options?\r\n        not @convert_options.blank?\r\n      end\r\n\
      \r\n      # Performs the conversion of the +file+ into a watermark. Returns the Tempfile\r\n      # that contains the new image.\r\n      def make\r\n        dst = Tempfile.new([@basename, @format].compact.join(\".\"))\r\n        dst.binmode\r\n\
      \r\n        if watermark_path\r\n          command = \"composite\"\r\n          # params = \"-gravity #{@position} #{watermark_path} #{fromfile} #{transformation_command} #{tofile(dst)}\"\r\n           params = %W[-gravity #{@position} #{watermark_path} #{fromfile}]\r\n           params += transformation_command\r\n           params << tofile(dst)\r\n        else\r\n          command = \"convert\"\r\n          # params = \"#{fromfile} #{transformation_command} #{tofile(dst)}\"\r\n          params = [fromfile]\r\n          params += transformation_command\r\n          params << tofile(dst)\r\n        end\r\n\
      \r\n        begin\r\n          # success = Paperclip.run(command, params)  \r\n          success = Paperclip.run(command, *params)\r\n        rescue PaperclipCommandLineError\r\n          raise PaperclipError, \"There was an error processing the watermark for #{@basename}\" if @whiny\r\n        end\r\n\
      \r\n        dst\r\n      end\r\n\
      \r\n      def fromfile\r\n        # \"\\\"#{ File.expand_path(@file.path) }[0]\\\"\"\r\n        File.expand_path(@file.path)\r\n      end\r\n\
      \r\n      def tofile(destination)\r\n        # \"\\\"#{ File.expand_path(destination.path) }[0]\\\"\"\r\n         File.expand_path(destination.path)\r\n      end\r\n\
      \r\n      def transformation_command\r\n        scale, crop = @current_geometry.transformation_to(@target_geometry, crop?)\r\n        # trans = \"-resize \\\"#{scale}\\\"\"\r\n        # trans << \" -crop \\\"#{crop}\\\" +repage\" if crop\r\n        # trans << \" #{convert_options}\" if convert_options?\r\n        trans = %W[-resize #{scale}]\r\n        trans += %W[-crop #{crop} +repage] if crop\r\n        trans << convert_options if convert_options?\r\n        trans\r\n      end\r\n\
      \r\n\
      \r\n  end\r\n\
      \r\n\
      end\r\n"
