      SELECT id, kbez , dynamik_kz , kuerzel, verarb_routine , ANW_NAME, TAC_NAME  FROM vw_glb1_dokument_strukturen        WHERE  KUERZEL = 'PERS'        ORDER BY SORT_NR;\r\n\
      --1. Ebene: Der Kunde\r\n\
      SELECT * FROM vw_bik4_dok_pers WHERE pk = 46804;  --> okay\r\n\
      --2. Ebene: die Entscheidungsvorlagen\r\n\
      SELECT * FROM vw_glb1_dokument_strukturen  WHERE  KUERZEL = 'EV' ORDER BY SORT_NR;        \r\n\
      --3. Ebene: die UFEV vom 11.05.2005      \r\n\
      SELECT * FROM vw_bik4_dok_ufb WHERE pk = 46804;   \r\n\
      \r\n\
      \r\n\
      -- die Datei:0013FF6E.pdf\r\n\
      SELECT * FROM dokument_zuordnungen WHERE server_dateiname = '0013FF6E.pdf'; \r\n\
      --> id = 1310574  , v_dokst_id = 22, v_dokar_id = 10, person_id = 46804, anwender Titel = General Information\r\n\
      \r\n\
      -- die korrekte Datei: 0004E023.pdf    \r\n\
      SELECT * FROM dokument_zuordnungen WHERE server_dateiname = '0004E023.pdf';  \r\n\
      -- Entscheidungsvorlage , v_dokst_id = 159       \r\n\
      \r\n\
      --> nachdem die v_dokst_id von 22 auf 159 angepasst wurde wird die Entscheidungsvorlage auch angezeigt\r\n\
      SELECT * FROM dokument_strukturen WHERE id = 158;\r\n\
      \r\n\
      SELECT * FROM vw_bik4_dok_sfb WHERE titel = 'SFEV  vom 17.07.2008'; \r\n\
      SELECT * FROM dokument_zuordnungen WHERE server_dateiname = '00114D3B.pdf'; \r\n\
      -- die Struktur Id muss zum Reiter sonstige UF passen, sonst wird die Datei nicht angezeigt.\r\n\
      BEGIN \r\n\
      pk_bik5_entscheidungsvorlage.pr_print_ev_xml(161039);\r\n\
      END; \r\n\
      \r\n\
      \r\n\
      SELECT min(jobauft_id)\r\n                          FROM vw_std0_batch_reports br1\r\n                         WHERE br1.PAKET_STATUS = 10-- c_n_PAKET_CLOSED_FOR_EXEC\r\n                           AND br1.JOB_STATUS = 0\r\n                          -- AND br1.PHASE =  i_n_PHASE\r\n                           AND br1.jobapak_id = nvl(3568,br1.jobapak_id)\r\n                           AND ( i_n_PHASE = c_vc_PHASE1 or\r\n                                 (SELECT count(*)\r\n                                    FROM vw_std0_batch_reports br2\r\n                                   WHERE br2.jobapak_id = br1.jobapak_id\r\n                                     AND br2.JOB_STATUS < c_n_JOB_SUCCESSFULL\r\n                                     AND br2.phase = c_vc_PHASE1 ) = 0  );\r\n\
      \r\n\
      SELECT * FROM job_auftraege WHERE id = 4633 ;--trunc(anl_dat) = trunc(sysdate);    \r\n\
      SELECT * FROM job_auftr_pakete WHERE id = 3568; \r\n\
      SELECT * FROM job_auftr_wertpaare WHERE jobauft_id IN (4633) AND schluessel = 'DO_SQL';\r\n\
      -- bis hierher ist der Schlüssel vorhanden\r\n\
      SELECT * FROM vw_std0_batch_reports WHERE jobapak_id = 3568;  \r\n\
      SELECT pk_std0_batch_report.fk_get_keyvalue(4631,'SQL_CODE','I') FROM dual;  \r\n    \r\n\
      SELECT   wert\r\n                   FROM job_auftr_wertpaare\r\n                  WHERE job_auftr_wertpaare.jobauft_id = 4633\r\n                    and job_auftr_wertpaare.intern_extern_kz = 'I'\r\n                    AND DECODE(intern_extern_kz\r\n                             , 'E', schluessel\r\n                             , 'I', SUBSTR(schluessel\r\n                                         , 1\r\n                                         , DECODE(INSTR(schluessel, '#')\r\n                                                , 0, LENGTH(schluessel)\r\n                                                , INSTR(schluessel, '#') - 1))) =\r\n                                                                        'SQL_CODE'\r\n               ORDER BY SUBSTR(schluessel\r\n                             , INSTR(schluessel, '#') + 1\r\n                             , LENGTH(schluessel) - INSTR(schluessel, '#')) ; \r\n\
      SELECT *  FROM   ev_druckdaten\r\n    WHERE  entschv_id  = 160153;     \r\n    \r\n begin  \r\n    EXECUTE IMMEDIATE (' begin pk_bik5_entscheidungsvorlage.pr_generate_ev_report(160153); dbms_output.put_line(:bindevariable); end; ') USING 4641;                                         \r\n END;\r\n \r\n\
      begin pk_bik5_entscheidungsvorlage.pr_generate_ev_report(161038); END;"
