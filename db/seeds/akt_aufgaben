    information: |-
      -- Entschiedene EVen ohne Bonigeber

      SELECT per.kd_nr,ev.id,ev.entsch_vom_dat,ev.typ,ev.art

      FROM entscheidungsvorlagen ev 

      JOIN personen per ON (per.id = ev.person_id)

      WHERE pk_sco9_rating_interface.fk_bonigeber_id(per.id) IS NULL

        AND ev.entsch_vom_dat > sysdate-200

        AND art <>3;
