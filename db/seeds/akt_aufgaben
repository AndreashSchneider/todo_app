      SELECT id,aend_dat,aend_user,typ,buchungskreis, produkt_nr,kd_nr,lfd_nr,tranche,

             saldo_lfd

        FROM produktverwendungen 

       WHERE TRUNC(aend_dat) = TRUNC(SYSDATE)

         AND prog_name = 'pk_ver6_sap'

         ORDER BY id;

      

