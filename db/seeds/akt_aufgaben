    id: "2934"
    information: |-
      mak_zustand      :=	pk_ris0_interface.fk_mak_zustand_kd||' '|...fk_mak_zustand_kd_stufe;

      mak_zustand_text :=	...fk_mak_zustand_kd_kbez(:BL_DLM_DEAL_DATEN.pers_id);

      

      SELECT kd_nr

           ,    pk_ris0_interface.fk_mak_zustand_kd_kbez(per.id)

             || ' '

             || pk_ris0_interface.fk_mak_zustand_kd_stufe(per.id)

           , pk_ris0_interface.fk_mak_zustand_kd_kbez(id)

        FROM personen per

       WHERE pk_ris0_interface.fk_mak_zustand_kd_stufe(per.id) IS NOT NULL

             AND EXISTS (SELECT 1

                           FROM produktverwendungen pvw

                          WHERE pvw.kd_nr = per.kd_nr

