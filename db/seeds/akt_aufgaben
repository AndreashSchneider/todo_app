- !ruby/object:Aufgabe 
  attributes: 
    status: Erledigt
    zeit: 
    aend_dat: 
    erfasst: 2008-12-08 00:00:00
    zustaendig: Herr Schneider
    nur_heute_interessant: N
    privat: N
    aufgabe: "Tuning der  PK_SID0_SYNTHETISCHE_UBS.fk_get_sid_entlastung durch Einbau der pl/sql-Table. An Herrn Hans Meyer gesandt. Im Gespräch mit Jürgen Laschewski wieder verworfen."
    mantis_nummer: 
    aufgabe_id: 
    id: "2132"
    information: |
      CREATE TABLE cov_daten_prod NOLOGGING AS SELECT * FROM vw_dlm0_covenant_daten

      ;
      CREATE INDEX cov_test_i1 ON cov_daten_prod(pro_Ver_Id,covenant_nr,stichquartal)

      ;
      CREATE INDEX cov_test_i2 ON cov_daten_prod(pro_Ver_Id,stichquartal)

      ;
       

       

      in PK_SID0_SYNTHETISCHE_UBS.fk_get_sid_entlastung pl/sql-Table eingebaut

      

      in Spec:

      TYPE tSid_entl IS TABLE OF NUMBER(21,2) INDEX BY PLS_INTEGER;

      

      tabSid_Entl tSid_entl;

      

       

      in Body:

       

      IF tabSid_Entl.EXISTS (i_pro_ver_id) THEN 

          n_return := tabSid_Entl(i_pro_ver_id);

      ELSE 

            FOR csid IN (SELECT shpevw.sh_sid_id

                         FROM   sh_pvw_verwendungen          shpvw

                                JOIN sh_person_verwendungen  shpevw   ON (shpevw.id = shpvw.sh_pevw_id)

                         WHERE  shpvw.besichert = 'J'

                           AND  shpvw.pro_ver_id = pk_sid0_rangfolge.fk_get_gzus_pro_ver_id(i_pro_ver_id)

                        )

            LOOP

               SELECT NVL(SUM(ROUND(NVL(wi_entla_hwae,0),2)),0)

               INTO   n_sum

               FROM   vw_sid0_ub_info

               WHERE  pvw_pro_ver_id = i_pro_ver_id

                 AND  sh_sid_id      = csid.sh_sid_id

                 AND  ub_pvw_bez_art = 'SYUB'

                 --AND  sid_aktuell    = NVL(v_aktuell, sid_aktuell)

                 --AND  sid_ist        = NVL(v_gueltig, sid_ist)

                 and  kred_sicht_gueltig_aktuell = UPPER(i_gueltig_aktuell)

                 ;

       

               n_return := n_return + n_sum;

       

            END LOOP;

      

            tabSid_Entl(i_pro_ver_id):= n_return;

      

      END IF;

       


    projekt: SF-Monitoring
